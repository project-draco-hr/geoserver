{
  File propFile=new File(getBaseDir(),CONFIG_FILE);
  try {
    propFile.createNewFile();
  }
 catch (  Exception e) {
    LOGGER.log(Level.WARNING,"Can't create file " + propFile.getAbsolutePath(),e);
    return;
  }
  try {
    OutputStream out=new FileOutputStream(propFile);
    try {
      config.store(out,comment);
    }
  finally {
      out.close();
    }
  }
 catch (  Exception e) {
    LOGGER.log(Level.WARNING,"Error saving jdbc config properties to file " + propFile.getAbsolutePath(),e);
    propFile.delete();
  }
}
