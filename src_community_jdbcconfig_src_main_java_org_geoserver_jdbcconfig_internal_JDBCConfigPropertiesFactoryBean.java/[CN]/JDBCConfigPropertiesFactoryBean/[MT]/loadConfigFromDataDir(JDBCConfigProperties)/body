{
  File propFile=new File(getBaseDir(),CONFIG_FILE);
  if (propFile.exists()) {
    LOGGER.info("Loading jdbcconfig properties from " + propFile.getAbsolutePath());
    FileInputStream stream=new FileInputStream(propFile);
    try {
      config.load(stream);
      return true;
    }
  finally {
      stream.close();
    }
  }
  return false;
}
