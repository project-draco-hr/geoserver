{
  super.populateDataDirectory(dataDirectory);
  dataDirectory.addWcs10Coverages();
  dataDirectory.addStyle("thickStroke",getClass().getResource("../wms_1_1_1/thickStroke.sld"));
  dataDirectory.addStyle("raster",getClass().getResource("../wms_1_1_1/raster.sld"));
  dataDirectory.addStyle("rasterScales",getClass().getResource("../wms_1_1_1/rasterScales.sld"));
  dataDirectory.addCoverage(TASMANIA_BM,getClass().getResource("../wms_1_1_1/tazbm.tiff"),"tiff","raster");
  dataDirectory.addStyle("squares",getClass().getResource("../wms_1_1_1/squares.sld"));
  dataDirectory.addPropertiesType(SQUARES,getClass().getResource("../wms_1_1_1/squares.properties"),null);
}
