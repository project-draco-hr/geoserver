{
  List<String> srsList=(List<String>)validatable.getValue();
  List<String> invalid=new ArrayList<String>();
  for (  String srs : srsList) {
    try {
      CRS.decode("EPSG:" + srs);
    }
 catch (    Exception e) {
      invalid.add(srs);
    }
  }
  if (invalid.size() > 0)   error(validatable,"SRSListTextArea.unknownEPSGCodes",Collections.singletonMap("codes",invalid.toString()));
}
