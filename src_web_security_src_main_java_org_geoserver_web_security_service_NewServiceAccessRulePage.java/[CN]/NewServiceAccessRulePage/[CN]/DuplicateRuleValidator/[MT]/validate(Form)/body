{
  ServiceAccessRule rule=new ServiceAccessRule((String)service.getConvertedInput(),(String)method.getConvertedInput(),rolesForComponent.getRolePalette().getDefaultModelObjectAsString());
  if (ServiceAccessRuleDAO.get().getRules().contains(rule)) {
    form.error(new ParamResourceModel("duplicateRule",getPage(),rule.getKey()).getString());
  }
}
