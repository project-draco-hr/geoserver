{
  if (!(o instanceof GeometryAttribute))   return false;
  GeometryAttribute other=(GeometryAttribute)o;
  if (srsName == null && other.srsName != null)   return false;
  if (srsName != null && srsName.equals(other.srsName) == false)   return false;
  return geometry.equalsExact(other.geometry);
}
