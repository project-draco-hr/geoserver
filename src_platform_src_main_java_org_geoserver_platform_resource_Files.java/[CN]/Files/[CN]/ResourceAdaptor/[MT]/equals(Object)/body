{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  ResourceAdaptor other=(ResourceAdaptor)obj;
  if (file == null) {
    if (other.file != null)     return false;
  }
 else   if (!file.equals(other.file))   return false;
  return true;
}
