{
  NamespaceInfo old=getDefaultNamespace();
  if (old != null) {
    ((NamespaceInfoImpl)old).setDefault(false);
    save(old);
  }
  if (namespace != null) {
    namespace=resolve(namespace);
    ((NamespaceInfoImpl)namespace).setDefault(true);
    save(namespace);
  }
  catalog.fireModified(catalog,Arrays.asList("defaultNamespace"),Arrays.asList(old),Arrays.asList(namespace));
}
