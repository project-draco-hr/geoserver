{
  this(imageStack,dx,dy,margin,LegendUtils.getBackgroundColor(req),req.isTransparent(),LegendUtils.isFontAntiAliasing(req),LegendUtils.getLayout(req),LegendUtils.getRowWidth(req),LegendUtils.getRows(req),LegendUtils.getColumnHeight(req),LegendUtils.getColumns(req),LegendUtils.getLabelFont(req),forceLabelsOn,forceLabelsOff);
}
