{
  Coordinate[] coordinates=new Coordinate[]{new Coordinate(env.getMinX(),env.getMinY()),new Coordinate(env.getMaxX(),env.getMinY()),new Coordinate(env.getMaxX(),env.getMaxY()),new Coordinate(env.getMinX(),env.getMaxY()),new Coordinate(env.getMinX(),env.getMinY())};
  LinearRing bbox=gFac.createLinearRing(coordinates);
  return gFac.createPolygon(bbox,new LinearRing[]{});
}
