{
  Document dom=getAsDOM("wms?bbox=" + bbox + "&styles="+ "&layers="+ layers+ "&Format=image/png"+ "&request=GetMap"+ "&width=550"+ "&height=250"+ "&srs=EPSG:4326"+ "&SLD_VERSION=1.0.0"+ "&SLD_BODY="+ STATES_SLD11.replaceAll("=","%3D")+ "&VALIDATESCHEMA=true");
