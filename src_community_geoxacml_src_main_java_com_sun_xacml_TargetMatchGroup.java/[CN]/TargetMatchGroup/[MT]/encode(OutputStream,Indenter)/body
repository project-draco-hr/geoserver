{
  PrintStream out=new PrintStream(output);
  String indent=indenter.makeString();
  String name=TargetMatch.NAMES[matchType];
  out.println(indent + "<" + name+ ">");
  indenter.in();
  for (  TargetMatch tm : matches) {
    tm.encode(output,indenter);
  }
  out.println(indent + "</" + name+ ">");
  indenter.out();
}
