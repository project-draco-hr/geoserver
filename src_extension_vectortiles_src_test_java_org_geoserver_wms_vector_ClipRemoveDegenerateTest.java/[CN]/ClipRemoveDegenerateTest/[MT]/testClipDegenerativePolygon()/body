{
  ClipRemoveDegenerateGeometries clip=new ClipRemoveDegenerateGeometries(new Envelope(0,10,0,10));
  Clip clip2=new Clip(new Envelope(0,10,0,10));
  Geometry result=clip._run(fromWKT("POLYGON( (-10 -10, 0 -10, 0 0,-10 0,-10 -10))"));
  assertNull(result);
  result=clip._run(fromWKT("POLYGON( (-10 -10, 10 -10, 10 0,-10 0,-10 -10))"));
  assertNull(result);
  result=clip._run(fromWKT("POLYGON( (-10 -10, 10 -10, 10 0,-10 0,-10 -10))"));
  assertNull(result);
  result=clip._run(fromWKT("POLYGON ((1  -3 , 1 0, 3 0, 3 -1, 5 -1, 5 0, 6 0, 7 -1, 10 -1, 10 0, 12 -3, 1 -3))"));
  assertNull(result);
  result=clip._run(fromWKT("POLYGON ((1 -3, 1 0, 3 0, 3 -1, 5 -1, 5 0, 6 0, 7 -1, 10 -1, 10 0, 10.993923885434695 -1.490885828152043, 11.834681129445318 2.6436334939202886, 8.332506124004496 3.936744265159977, 13.397189978026608 4.313901573438219, 12 -3, 1 -3))"));
  assertEquals(result,fromWKT("POLYGON ((10 3.32105421863856, 8.332506124004496 3.936744265159977, 10 4.060919341031982, 10 3.32105421863856)))"));
  result=clip._run(fromWKT("POLYGON ((1 -3, 1 0, 3 0, 3 -1, 5 -1, 5 0, 6 0, 7 -1, 10 -1, 10 0, 10.993923885434695 -1.490885828152043, 11.834681129445318 2.6436334939202886, 8.332506124004496 3.936744265159977, 11.242005359293794 4.744938497184782, 9.03294112509266 6.576845423107674, 12.050199591318599 6.900123115917596, 13.397189978026608 4.313901573438219, 12 -3, 1 -3))"));
  assertEquals(result,fromWKT("MULTIPOLYGON (((10 3.32105421863856, 8.332506124004496 3.936744265159977, 10 4.399937008492062, 10 3.32105421863856)), ((10 5.774894160989392, 9.03294112509266 6.576845423107674, 10 6.680458873990603, 10 5.774894160989392)))"));
}
