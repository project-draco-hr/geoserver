{
  final File file=new File(demoDir,reqFileName);
  final StringBuilder sb=new StringBuilder();
  final BufferedReader reader=new BufferedReader(new FileReader(file));
  String line;
  try {
    while ((line=reader.readLine()) != null) {
      sb.append(line);
      sb.append("\n");
    }
  }
  finally {
    reader.close();
  }
  return sb.toString();
}
