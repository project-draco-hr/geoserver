{
  initLdapServer(false);
  config.setUserDnPattern("uid={0},ou=People");
  config.setBindBeforeGroupSearch(false);
  createAuthenticationProvider();
  boolean error=false;
  try {
    authProvider.authenticate(authentication);
  }
 catch (  Exception e) {
    error=true;
  }
  assertTrue(error);
}
