{
  super(id,new Model());
  requestChain=model.getObject().getRequestChainByName("web");
  add(new RequestChainDropDownChoice("requestChain",new PropertyModel(this,"requestChain"),new PropertyModel<List<RequestFilterChain>>(model,"requestChains")).add(new OnChangeAjaxBehavior(){
    @Override protected void onUpdate(    AjaxRequestTarget target){
      target.addComponent(AuthFilterChainPanel.this.get("authFilterChain"));
    }
  }
));
  add(new AuthFilterChainPalette("authFilterChain",new AuthFilterNamesModel(model)).setOutputMarkupId(true));
}
