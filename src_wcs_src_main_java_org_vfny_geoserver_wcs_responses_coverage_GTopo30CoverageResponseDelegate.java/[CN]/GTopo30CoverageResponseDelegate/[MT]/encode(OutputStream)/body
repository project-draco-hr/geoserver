{
  final ZipOutputStream outZ=new ZipOutputStream(output);
  output=outZ;
  final GridCoverageWriter writer=new GTopo30Writer(outZ);
  try {
    writer.write(sourceCoverage,null);
  }
  finally {
    try {
      writer.dispose();
    }
 catch (    Throwable e) {
    }
    this.sourceCoverage.dispose(false);
    this.sourceCoverage=null;
  }
}
