{
  StringBuilder description=new StringBuilder(template.description(feature));
  try {
    Class.forName("org.geoserver.geosearch.LayerAboutPage");
    description.append("<div> <a href=\"").append(getFeatureTypeURL()).append(".html").append("\">Full dataset info and download</a> </div>");
  }
 catch (  ClassNotFoundException cnfe) {
  }
  if (description != null) {
    start("description");
    cdata(description.toString());
    end("description");
  }
}
