{
  if (value == null || "".equals(value)) {
    return null;
  }
  try {
    try {
      URL url=new URL(value);
      if ("file".equals(url.getProtocol())) {
        value=url.getFile();
      }
    }
 catch (    MalformedURLException e) {
    }
    File file=new File(value);
    if (file.isAbsolute()) {
      return file;
    }
    return resourceLoader.find(value);
  }
 catch (  IOException e) {
  }
  return null;
}
