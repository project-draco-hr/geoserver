{
  if (functionName.equals(NAME_REGEXP_STRING_MATCH))   return regexpParams;
 else   if (functionName.equals(NAME_X500NAME_MATCH))   return x500Params;
 else   if (functionName.equals(NAME_RFC822NAME_MATCH))   return rfc822Params;
 else   if (functionName.equals(NAME_STRING_REGEXP_MATCH))   return stringRegexpParams;
 else   if (functionName.equals(NAME_ANYURI_REGEXP_MATCH))   return anyURIRegexpParams;
 else   if (functionName.equals(NAME_IPADDRESS_REGEXP_MATCH))   return ipAddressRegexpParams;
 else   if (functionName.equals(NAME_DNSNAME_REGEXP_MATCH))   return dnsNameRegexpParams;
 else   if (functionName.equals(NAME_RFC822NAME_REGEXP_MATCH))   return rfc822NameRegexpParams;
 else   if (functionName.equals(NAME_X500NAME_REGEXP_MATCH))   return x500NameRegexpParams;
  return null;
}
