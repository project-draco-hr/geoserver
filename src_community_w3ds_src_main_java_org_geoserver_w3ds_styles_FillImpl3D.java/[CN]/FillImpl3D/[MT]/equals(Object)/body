{
  if (this == oth) {
    return true;
  }
  if (oth instanceof FillImpl) {
    FillImpl other=(FillImpl)oth;
    return Utilities.equals(this.color,other.getColor()) && Utilities.equals(this.backgroundColor,other.getBackgroundColor()) && Utilities.equals(this.opacity,other.getOpacity())&& Utilities.equals(this.graphicFill,other.getGraphicFill());
  }
  return false;
}
