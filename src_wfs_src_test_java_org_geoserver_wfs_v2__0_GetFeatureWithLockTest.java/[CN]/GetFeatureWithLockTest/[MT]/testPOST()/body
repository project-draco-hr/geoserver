{
  String xml="<wfs:GetFeatureWithLock service='WFS' version='2.0.0' " + "handle='GetFeatureWithLock-tc1' expiry='5' resultType='results' " + "xmlns:sf='http://cite.opengeospatial.org/gmlsf' xmlns:wfs='" + WFS.NAMESPACE + "'>"+ "<wfs:Query handle='qry-1' typeNames='sf:PrimitiveGeoFeature' />"+ "</wfs:GetFeatureWithLock>";
  Document dom=postAsDOM("wfs",xml);
  assertGML32(dom);
  assertNotNull(dom.getDocumentElement().getAttribute("lockId"));
}
