{
  PythonFilterFunctionAdapter adapter=adapters.get(name);
  if (adapter == null) {
synchronized (this) {
      adapter=adapters.get(name);
      if (adapter == null) {
        try {
          adapter=createFilterFunctionAdapter(name);
        }
 catch (        IOException e) {
          throw new RuntimeException(e);
        }
        if (adapter != null) {
          adapters.put(name,adapter);
        }
      }
    }
  }
  return adapter;
}
