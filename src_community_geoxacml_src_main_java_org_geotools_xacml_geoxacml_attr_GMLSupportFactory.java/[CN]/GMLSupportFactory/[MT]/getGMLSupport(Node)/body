{
  if ("Box".equals(node.getLocalName()))   return GML2Support.Singleton;
  if ("MultiLineString".equals(node.getLocalName()))   return GML2Support.Singleton;
  if ("MultiPolygon".equals(node.getLocalName()))   return GML2Support.Singleton;
  if ("Envelope".equals(node.getLocalName()))   return GML3Support.Singleton;
  if ("MultiCurve".equals(node.getLocalName()))   return GML3Support.Singleton;
  if ("MultiSurface".equals(node.getLocalName()))   return GML3Support.Singleton;
  if ("Polygon".equals(node.getLocalName())) {
    if (gmlElemExists(node,"exterior"))     return GML3Support.Singleton;
    if (gmlElemExists(node,"outerBoundaryIs"))     return GML2Support.Singleton;
  }
  if (gmlElemExists(node,"posList") || gmlElemExists(node,"pos"))   return GML3Support.Singleton;
 else   return GML2Support.Singleton;
}
