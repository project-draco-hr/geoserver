{
  Document dom=getAsDOM("wfs?request=GetFeature&version=1.1.0&service=wfs&typename=" + getLayerId(MockData.PRIMITIVEGEOFEATURE));
  XMLAssert.assertXpathExists("//gml:Point[@srsDimension = '2']",dom);
  WFSInfo wfs=getWFS();
  wfs.setCiteCompliant(true);
  getGeoServer().save(wfs);
  dom=getAsDOM("wfs?request=GetFeature&version=1.1.0&service=wfs&typename=" + getLayerId(MockData.PRIMITIVEGEOFEATURE));
  XMLAssert.assertXpathNotExists("//gml:Point[@srsDimension = '2']",dom);
}
