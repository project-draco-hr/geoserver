{
switch (xacmlDecision) {
case Result.DECISION_DENY:
    return AccessDecisionVoter.ACCESS_DENIED;
case Result.DECISION_PERMIT:
  return AccessDecisionVoter.ACCESS_GRANTED;
case Result.DECISION_NOT_APPLICABLE:
return mappingForNotApplicable;
case Result.DECISION_INDETERMINATE:
throw new RuntimeException("Never should reach this point, no existing spring security mapping for xacml DECISION_INDETERMINATE");
}
throw new RuntimeException("Never should reach this point");
}
