{
  for (  String sf : supportedFormats) {
    if (sf.equalsIgnoreCase(format.trim())) {
      return sf;
    }
 else {
      CoverageResponseDelegate delegate=CoverageResponseDelegateFactory.encoderFor(sf);
      if (delegate != null && delegate.canProduce(format))       return sf;
    }
  }
  return null;
}
