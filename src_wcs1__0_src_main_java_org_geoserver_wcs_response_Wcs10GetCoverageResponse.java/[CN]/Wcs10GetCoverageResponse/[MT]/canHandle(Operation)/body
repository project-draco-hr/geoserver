{
  if (!(operation.getParameters()[0] instanceof GetCoverageType))   return false;
  GetCoverageType getCoverage=(GetCoverageType)operation.getParameters()[0];
  String outputFormat=getCoverage.getOutput().getFormat().getValue();
  if (delegate == null)   this.delegate=CoverageResponseDelegateFactory.encoderFor(outputFormat);
  if (delegate == null)   throw new WcsException("Could not find encoder for output format " + outputFormat);
  return delegate.canProduce(outputFormat);
}
