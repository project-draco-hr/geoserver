{
  try {
    checkEmpty(service);
    checkEmpty(store);
    insertValues(store);
    store.store();
    checkValuesInserted(store);
    checkValuesInserted(service);
    removeValues(store);
    if (!isJDBCTest())     checkValuesInserted(service);
    checkValuesRemoved(store);
    store.load();
    checkValuesInserted(store);
    checkValuesInserted(service);
    removeValues(store);
    store.store();
    checkValuesRemoved(store);
    checkValuesRemoved(service);
  }
 catch (  IOException ex) {
    Assert.fail(ex.getMessage());
  }
}
