{
  StringBuffer sb=new StringBuffer();
  sb.append("DateTimeAttribute: [\n");
  sb.append("  Date: " + value + " local time");
  sb.append("  Nanoseconds: " + nanoseconds);
  sb.append("  TimeZone: " + timeZone);
  sb.append("  Defaulted TimeZone: " + defaultedTimeZone);
