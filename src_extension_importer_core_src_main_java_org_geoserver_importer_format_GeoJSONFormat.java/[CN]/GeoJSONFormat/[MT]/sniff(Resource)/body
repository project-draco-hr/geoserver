{
  try {
    FeatureIterator it=new FeatureJSON().streamFeatureCollection(file.file());
    try {
      if (it.hasNext()) {
        return (SimpleFeature)it.next();
      }
    }
  finally {
      it.close();
    }
  }
 catch (  Exception e) {
    LOG.log(Level.FINER,"Error reading file as json",e);
  }
  return null;
}
