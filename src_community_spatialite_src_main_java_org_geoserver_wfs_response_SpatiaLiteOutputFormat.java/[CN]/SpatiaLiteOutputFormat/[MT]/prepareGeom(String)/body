{
  String value=theGeom;
  if (theGeom.contains("MULTIPOINT")) {
    value=value.replaceAll("\\(\\(","*");
    value=value.replaceAll("\\)\\)","#");
    value=value.replaceAll("\\(","");
    value=value.replaceAll("\\)","");
    value=value.replaceAll("\\*","(");
    value=value.replaceAll("\\#",")");
  }
  return value;
}
