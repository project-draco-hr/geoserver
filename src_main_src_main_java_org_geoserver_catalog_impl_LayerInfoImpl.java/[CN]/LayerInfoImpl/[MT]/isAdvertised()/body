{
  if (this.advertised != null) {
    return advertised;
  }
  MetadataMap md=getMetadata();
  if (md == null) {
    return true;
  }
  Boolean metadataAdvertised=md.get(KEY_ADVERTISED,Boolean.class);
  if (metadataAdvertised == null) {
    metadataAdvertised=true;
  }
  return metadataAdvertised;
}
