{
  String xml="<wps:Execute service='WPS' version='1.0.0' xmlns:wps='http://www.opengis.net/wps/1.0.0' " + "xmlns:ows='http://www.opengis.net/ows/1.1'>" + "<ows:Identifier>gt:buffer</ows:Identifier>"+ "<wps:DataInputs>"+ "<wps:Input>"+ "<ows:Identifier>geom1</ows:Identifier>"+ "<wps:Data>"+ "<wps:ComplexData>"+ "<gml:Polygon xmlns:gml='http://www.opengis.net/gml'>"+ "<gml:exterior>"+ "<gml:LinearRing>"+ "<gml:coordinates>1 1 2 1 2 2 1 2 1 1</gml:coordinates>"+ "</gml:LinearRing>"+ "</gml:exterior>"+ "</gml:Polygon>"+ "</wps:ComplexData>"+ "</wps:Data>"+ "</wps:Input>"+ "<wps:Input>"+ "<ows:Identifier>buffer</ows:Identifier>"+ "<wps:Data>"+ "<wps:LiteralData>1</wps:LiteralData>"+ "</wps:Data>"+ "</wps:Input>"+ "</wps:DataInputs>"+ "<wps:ResponseForm>"+ "    <wps:RawDataOutput>"+ "        <ows:Identifier>result</ows:Identifier>"+ "    </wps:RawDataOutput>"+ "  </wps:ResponseForm>"+ "</wps:Execute>";
  Document d=postAsDOM("wps",xml);
  checkValidationErrors(d,new GMLConfiguration());
  assertEquals("gml:Polygon",d.getDocumentElement().getNodeName());
}
