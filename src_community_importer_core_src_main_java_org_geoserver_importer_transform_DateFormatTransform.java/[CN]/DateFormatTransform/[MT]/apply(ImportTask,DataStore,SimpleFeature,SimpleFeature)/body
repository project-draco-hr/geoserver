{
  Object val=feature.getAttribute(field);
  if (val != null) {
    Date parsed=parseDate(val.toString());
    if (parsed == null) {
      task.addMessage(Level.WARNING,"Invalid date '" + val + "' specified for "+ feature.getID());
      feature=null;
    }
 else {
      feature.setAttribute(field,parsed);
    }
  }
  return feature;
}
