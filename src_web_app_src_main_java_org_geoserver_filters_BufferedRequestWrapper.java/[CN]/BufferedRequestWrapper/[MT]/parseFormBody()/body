{
  myParameterMap=new TreeMap();
  String[] pairs=myBuffer.split("\\&");
  for (int i=0; i < pairs.length; i++) {
    parsePair(pairs[i]);
  }
  if (myWrappedRequest.getQueryString() != null) {
    pairs=myWrappedRequest.getQueryString().split("\\&");
    for (int i=0; i < pairs.length; i++) {
      parsePair(pairs[i]);
    }
  }
}
