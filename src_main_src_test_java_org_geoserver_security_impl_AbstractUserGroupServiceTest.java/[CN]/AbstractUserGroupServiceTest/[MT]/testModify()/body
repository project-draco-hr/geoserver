{
  try {
    checkEmpty(service);
    checkEmpty(store);
    insertValues(store);
    store.store();
    checkValuesInserted(store);
    checkValuesInserted(service);
    modifyValues(store);
    if (!isJDBCTest())     checkValuesInserted(service);
    checkValuesModified(store);
    store.load();
    checkValuesInserted(store);
    checkValuesInserted(service);
    modifyValues(store);
    store.store();
    checkValuesModified(store);
    checkValuesModified(service);
  }
 catch (  Exception ex) {
    Assert.fail(ex.getMessage());
  }
}
