{
  List<Property<Summary>> props=new ArrayList<Property<Summary>>();
  props.add(new AbstractProperty<Summary>("Name"){
    public Object getPropertyValue(    Summary sum){
      return sum.getName();
    }
  }
);
  props.add(new AbstractProperty<Summary>("Minimum"){
    public Object getPropertyValue(    Summary sum){
      return sum.getMin();
    }
  }
);
  props.add(new AbstractProperty<Summary>("Maximum"){
    public Object getPropertyValue(    Summary sum){
      return sum.getMax();
    }
  }
);
  return props;
}
