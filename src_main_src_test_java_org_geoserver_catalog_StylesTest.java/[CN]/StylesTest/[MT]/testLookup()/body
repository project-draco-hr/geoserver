{
  assertTrue(Styles.handler(SLDHandler.FORMAT) instanceof SLDHandler);
  assertTrue(Styles.handler(PropertyStyleHandler.FORMAT) instanceof PropertyStyleHandler);
  try {
    Styles.handler(null);
    fail();
  }
 catch (  Exception e) {
  }
  try {
    Styles.handler("foo");
    fail();
  }
 catch (  Exception e) {
  }
}
