{
  Iterator<Item<SecurityNamedServiceConfig>> it=getDataView().getItems();
  while (it.hasNext()) {
    Item<SecurityNamedServiceConfig> item=it.next();
    if (name.equals(item.getModelObject().getName()))     tester.executeAjaxEvent(item.getPageRelativePath() + ":" + CHECKBOX_PATH,"onclick");
  }
}
