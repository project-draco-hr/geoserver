{
  if (event.getSource() instanceof StyleInfo) {
    try {
      initializeStyles();
    }
 catch (    RuntimeException e) {
      if (LOGGER.isLoggable(Level.WARNING)) {
        LOGGER.log(Level.WARNING,e.getMessage(),e);
      }
    }
catch (    Exception e) {
      if (LOGGER.isLoggable(Level.WARNING)) {
        LOGGER.log(Level.WARNING,e.getMessage(),e);
      }
    }
  }
}
