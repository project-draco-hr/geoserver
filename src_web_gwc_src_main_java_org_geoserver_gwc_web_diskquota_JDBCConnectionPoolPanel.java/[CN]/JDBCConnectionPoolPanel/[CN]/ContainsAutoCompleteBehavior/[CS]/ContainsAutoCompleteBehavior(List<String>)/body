{
  super(new AbstractAutoCompleteTextRenderer<String>(){
    private static final long serialVersionUID=3192368880726583011L;
    @Override protected String getTextValue(    String object){
      return object;
    }
  }
);
  settings.setPreselect(true).setShowListOnEmptyInput(true).setShowCompleteListOnFocusGain(true);
  this.choices=new ArrayList<String>(choices);
}
