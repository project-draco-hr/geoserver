{
  GetFeatureType request=(GetFeatureType)OwsUtils.parameter(operation.getParameters(),GetFeatureType.class);
  String outputFileName=((QName)((QueryType)request.getQuery().get(0)).getTypeName().get(0)).getLocalPart();
  return (String[][])new String[][]{{"Content-Disposition","attachment; filename=" + outputFileName + ".csv"}};
}
