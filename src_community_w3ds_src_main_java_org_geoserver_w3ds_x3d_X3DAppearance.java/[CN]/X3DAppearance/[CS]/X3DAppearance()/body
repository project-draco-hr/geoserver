{
  this.appearance=new X3DNode("Appearance");
  this.def=new X3DAttribute("DEF","");
  this.def.setValid(false);
  setDefaultMaterial();
  setTextureDefault();
  this.appearance.addX3DNode(imageTexture);
  this.appearance.addX3DNode(material);
  this.name="";
  this.filter=null;
  this.rule=null;
}
