{
  String request="csw?service=CSW&version=2.0.2&request=GetRecords&typeNames=csw:Record&resultType=results" + "&constraint=BBOX(ows:BoundingBox, -250, -250, -190, -100)";
  Document d=getAsDOM(request);
  checkValidationErrors(d,new CSWConfiguration());
  print(d);
  assertXpathExists("//csw:SummaryRecord[dc:title='Forests']",d);
}
