{
  MapLayer[] layers=mapContext.getLayers();
  for (int i=0; i < layers.length; i++) {
    if (layers[i] == layer) {
      return isRequestGWCCompatible(mapContext.getRequest(),i,wms);
    }
  }
  LOGGER.warning("Could not find map layer " + layer.getTitle() + " in the map context");
  return false;
}
