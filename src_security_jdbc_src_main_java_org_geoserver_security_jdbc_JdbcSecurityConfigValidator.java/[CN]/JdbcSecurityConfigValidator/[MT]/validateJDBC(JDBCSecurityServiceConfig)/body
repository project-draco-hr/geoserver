{
  if (isNotEmpty(config.getDriverClassName()) == false)   throw createSecurityException(SEC_ERR_200);
  if (isNotEmpty(config.getUserName()) == false)   throw createSecurityException(SEC_ERR_201);
  if (isNotEmpty(config.getConnectURL()) == false)   throw createSecurityException(SEC_ERR_202);
  try {
    Class.forName(config.getDriverClassName());
  }
 catch (  ClassNotFoundException e) {
    throw createSecurityException(SEC_ERR_203,config.getDriverClassName());
  }
}
