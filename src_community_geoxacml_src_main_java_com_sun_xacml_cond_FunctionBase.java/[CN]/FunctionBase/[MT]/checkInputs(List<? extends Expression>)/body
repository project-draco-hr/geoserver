{
  if (singleType) {
    if (numParams != -1) {
      if (inputs.size() != numParams)       throw new IllegalArgumentException("wrong number of args" + " to " + functionName);
    }
 else {
      if (inputs.size() < minParams)       throw new IllegalArgumentException("not enough args" + " to " + functionName);
    }
    Iterator<? extends Expression> it=inputs.iterator();
    while (it.hasNext()) {
      Evaluatable eval=(Evaluatable)(it.next());
      if ((!eval.getType().toString().equals(paramType)) || (eval.returnsBag() != paramIsBag))       throw new IllegalArgumentException("illegal parameter");
    }
  }
 else {
    if (paramTypes.length != inputs.size())     throw new IllegalArgumentException("wrong number of args" + " to " + functionName);
    Iterator<? extends Expression> it=inputs.iterator();
    int i=0;
    while (it.hasNext()) {
      Evaluatable eval=(Evaluatable)(it.next());
      if ((!eval.getType().toString().equals(paramTypes[i])) || (eval.returnsBag() != paramsAreBags[i]))       throw new IllegalArgumentException("illegal parameter");
      i++;
    }
  }
}
