{
  Iterator<? extends Expression> it=params.iterator();
  int index=0;
  while (it.hasNext()) {
    Evaluatable eval=(Evaluatable)(it.next());
    EvaluationResult result=eval.evaluate(context);
    if (result.indeterminate())     return result;
    args[index++]=result.getAttributeValue();
  }
  return null;
}
