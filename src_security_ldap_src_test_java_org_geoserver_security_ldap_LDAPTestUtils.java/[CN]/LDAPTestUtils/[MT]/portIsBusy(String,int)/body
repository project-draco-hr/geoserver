{
  Socket socket=new Socket();
  InetSocketAddress endPoint=new InetSocketAddress(host,port);
  if (endPoint.isUnresolved()) {
    return false;
  }
 else {
    try {
      socket.connect(endPoint,2000);
      return true;
    }
 catch (    IOException ioe) {
      return false;
    }
 finally {
      try {
        socket.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
