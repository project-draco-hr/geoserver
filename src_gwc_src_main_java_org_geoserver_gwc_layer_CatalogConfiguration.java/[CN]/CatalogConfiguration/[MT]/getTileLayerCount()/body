{
  int count=0;
  lock.readLock().lock();
  try {
    Set<String> layerIds=tileLayerCatalog.getLayerIds();
    if (pendingDeletes.isEmpty()) {
      count=layerIds.size();
    }
 else {
      for (      String layerId : layerIds) {
        if (pendingDeletes.contains(layerId)) {
          continue;
        }
        ++count;
      }
    }
  }
  finally {
    lock.readLock().unlock();
  }
  return count;
}
