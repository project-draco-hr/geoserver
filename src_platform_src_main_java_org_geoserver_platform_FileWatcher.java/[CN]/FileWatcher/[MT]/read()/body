{
  T result=null;
  if (file.exists()) {
    InputStream is=null;
    try {
      is=new FileInputStream(file);
      result=parseFileContents(is);
      lastModified=file.lastModified();
      lastCheck=System.currentTimeMillis();
      stale=false;
    }
  finally {
      if (is != null) {
        is.close();
      }
    }
  }
  return result;
}
