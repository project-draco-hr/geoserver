{
  if (m_BaseDataObject != null) {
    try {
      FeatureCollection<SimpleFeatureType,SimpleFeature> features=getFeatureSource().getFeatures(m_Query);
      return new GTFeatureIterator(features);
    }
 catch (    Exception e) {
      e.printStackTrace();
      return null;
    }
  }
 else {
    return null;
  }
}
