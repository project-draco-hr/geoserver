{
  VFSWorker vfs=new VFSWorker();
  if (vfs.canHandle(file)) {
    LOGGER.fine("unpacking " + file.path() + " to "+ this.file.path());
    vfs.extractTo(file,this.file);
    LOGGER.fine("deleting " + file.path());
    if (!file.delete()) {
      throw new IOException("unable to delete file");
    }
  }
}
