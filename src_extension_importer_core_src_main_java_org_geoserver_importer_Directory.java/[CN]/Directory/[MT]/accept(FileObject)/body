{
  FileName name=fo.getName();
  String localName=name.getBaseName();
  Resource dest=child(localName);
  FileObject dfo=null;
  try {
    dfo=VFS.getManager().resolveFile(dest.file().getAbsolutePath());
    dfo.copyFrom(fo,new AllFileSelector());
    unpack(dest);
  }
  finally {
    if (dfo != null) {
      dfo.close();
    }
  }
}
