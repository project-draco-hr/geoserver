{
  File[] files=file.listFiles();
  if (files != null) {
    for (    File f : files) {
      if (f.isDirectory()) {
        new Directory(f).cleanup();
      }
 else {
        if (LOGGER.isLoggable(Level.FINE)) {
          LOGGER.fine("Deleting file " + f.getAbsolutePath());
        }
        f.delete();
      }
    }
  }
  super.cleanup();
}
