{
  Object[] list=inputs.toArray();
  if (list.length == 0)   throw new IllegalArgumentException("n-of requires an argument");
  Evaluatable eval=(Evaluatable)(list[0]);
  if (!eval.getType().toString().equals(IntegerAttribute.identifier))   throw new IllegalArgumentException("first argument to n-of must" + " be an integer");
  for (int i=1; i < list.length; i++) {
    if (!((Evaluatable)(list[i])).getType().toString().equals(BooleanAttribute.identifier))     throw new IllegalArgumentException("invalid parameter in n-of" + ": expected boolean");
  }
}
