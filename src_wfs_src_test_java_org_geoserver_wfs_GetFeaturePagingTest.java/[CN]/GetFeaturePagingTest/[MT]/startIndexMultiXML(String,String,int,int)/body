{
  String xml="<GetFeature version=\"1.0.0\" xmlns:gml=\"http://www.opengis.net/gml\" startIndex='" + startIndex + "'";
  if (maxFeatures > -1) {
    xml+=" maxFeatures='" + maxFeatures + "'";
  }
  xml+=">" + " <Query typeName='" + fifteen + "'>"+ " </Query>"+ " <Query typeName='"+ seven+ "'>"+ " </Query>"+ "</GetFeature>";
  return xml;
}
