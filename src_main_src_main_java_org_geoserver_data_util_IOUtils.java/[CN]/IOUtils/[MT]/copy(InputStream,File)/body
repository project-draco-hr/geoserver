{
  OutputStream out=null;
  try {
    out=new FileOutputStream(to);
    byte[] buffer=new byte[1024 * 16];
    int bytes=0;
    while ((bytes=from.read(buffer)) != -1)     out.write(buffer,0,bytes);
    out.flush();
  }
  finally {
    if (from != null) {
      from.close();
    }
    if (out != null) {
      out.close();
    }
  }
}
