{
  RequestCtx request=null;
  try {
    request=RequestCtx.getInstance(new FileInputStream(TestSupport.getGeoXACMLFNFor("wildcard","WildCardRequest.xml")));
  }
 catch (  Exception e) {
    e.printStackTrace();
    Assert.fail(e.getMessage());
  }
  RequestCtx request1=null;
  try {
    request1=RequestCtx.getInstance(new FileInputStream(TestSupport.getGeoXACMLFNFor("wildcard","WildCardRequest1.xml")));
  }
 catch (  Exception e) {
    e.printStackTrace();
    Assert.fail(e.getMessage());
  }
  List<RequestCtx> resultList=new ArrayList<RequestCtx>();
  for (int i=0; i < 10; i++) {
    resultList.add(request);
    resultList.add(request1);
  }
  return resultList;
}
