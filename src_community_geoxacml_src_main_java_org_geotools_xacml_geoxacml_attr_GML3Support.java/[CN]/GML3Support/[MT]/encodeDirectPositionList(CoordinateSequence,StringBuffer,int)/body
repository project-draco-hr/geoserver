{
  encodeStartTag("posList",buff,null);
  for (int i=0; i < s.size(); i++) {
    Coordinate c=s.getCoordinate(i);
    if (i > 0)     buff.append(" ");
    buff.append(c.x);
    if (srsDimension == 2)     buff.append(" ").append(c.y);
    if (srsDimension == 3)     buff.append(" ").append(c.z);
  }
  encodeEndTag("posList",buff);
}
