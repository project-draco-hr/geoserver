{
  double[] oords=parseDoubleList(node);
  if (srsDimension == 1)   return new Coordinate(oords[0],Double.NaN);
  if (srsDimension == 2)   return new Coordinate(oords[0],oords[1]);
  if (srsDimension == 3)   return new Coordinate(oords[0],oords[1],oords[2]);
  throw new GMLException("Cannot parse DirectPosition");
}
