{
  double[] values=parseDoubleList(node);
  int coordinatesCount=values.length / srsDimension;
  Coordinate[] array=new Coordinate[coordinatesCount];
  for (int i=0; i < coordinatesCount; i++) {
    int offset=i * srsDimension;
    if (srsDimension == 1)     array[i]=new Coordinate(values[offset],Double.NaN,Double.NaN);
    if (srsDimension == 2)     array[i]=new Coordinate(values[offset],values[offset + 1],Double.NaN);
    if (srsDimension == 3)     array[i]=new Coordinate(values[offset],values[offset + 1],values[offset + 2]);
  }
  return csFactory.create(array);
}
