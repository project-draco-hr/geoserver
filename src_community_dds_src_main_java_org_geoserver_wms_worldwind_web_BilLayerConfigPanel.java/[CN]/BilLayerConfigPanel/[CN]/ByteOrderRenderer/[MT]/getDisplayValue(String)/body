{
  IStringResourceLoader loader=new GeoServerStringResourceLoader();
  if (ByteOrder.LITTLE_ENDIAN.toString().equals(str)) {
    return loader.loadStringResource(BilLayerConfigPanel.this,"BilLayerConfigPanel.byteOrderLittleEndian");
  }
 else   if (ByteOrder.BIG_ENDIAN.toString().equals(str)) {
    return loader.loadStringResource(BilLayerConfigPanel.this,"BilLayerConfigPanel.byteOrderBigEndian");
  }
  LOGGER.warning("Unknown byte order: " + str);
  return str;
}
