{
  LOGGER.warning("Rel14DXFWriter.writeTables");
  writeSectionStart("TABLES");
  writeViewPort(featureList);
  writeLineTypes();
  writeLayers(featureList);
  writeStyles();
  writeView();
  writeUCS();
  writeApplications();
  writeDimensionStyles();
  writeBlockRecords(featureList);
  writeSectionEnd();
}
