{
  assertActivatedKeyStore();
  FileOutputStream fos=new FileOutputStream(getFile());
  char[] passwd=securityManager.getMasterPassword();
  try {
    ks.store(fos,passwd);
  }
 catch (  Exception e) {
    throw new IOException(e);
  }
 finally {
    securityManager.disposePassword(passwd);
  }
  fos.close();
}
