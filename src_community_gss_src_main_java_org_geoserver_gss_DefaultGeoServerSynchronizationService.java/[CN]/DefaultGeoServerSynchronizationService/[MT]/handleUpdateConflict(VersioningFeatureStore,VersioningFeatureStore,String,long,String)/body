{
  SimpleFeature conflict=SimpleFeatureBuilder.build(conflicts.getSchema(),new Object[]{layer.getSchema().getTypeName(),id.substring(id.lastIndexOf(".") + 1),newLocalRevision,new Date(),'c',null,toGML3(getFeatureById(layer,id))},null);
  conflicts.addFeatures(collection(conflict));
  Id filter=ff.id(Collections.singleton(ff.featureId(id)));
  layer.rollback(lastLocalRevisionId,filter,null);
}
