{
  for (  String option : options) {
    cmd.addArgument(option);
  }
  if (inline) {
    cmd.addArgument("${input}");
  }
 else {
    if (isOutputAfterInput()) {
      cmd.addArgument("${input}");
      cmd.addArgument("${output}");
    }
 else {
      cmd.addArgument("${output}");
      cmd.addArgument("${input}");
    }
  }
}
