{
  if (ns == null) {
    ns=rd.getFeatureType().getName().getNamespaceURI();
  }
  String prefix=namespaces.getPrefix(ns);
  String xpath;
  if (prefix != null && !"".equals(prefix)) {
    xpath=prefix + ":" + localName;
  }
 else {
    xpath=localName;
  }
  PropertyName property=FF.property(xpath,namespaces);
  return property;
}
