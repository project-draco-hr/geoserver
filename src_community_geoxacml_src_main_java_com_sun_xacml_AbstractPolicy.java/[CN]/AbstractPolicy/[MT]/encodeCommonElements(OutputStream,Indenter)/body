{
  for (  CombinerElement elem : childElements)   elem.encode(output,indenter);
  if (obligations.size() != 0) {
    PrintStream out=new PrintStream(output);
    String indent=indenter.makeString();
    out.println(indent + "<Obligations>");
    indenter.in();
    for (    Obligation obligation : obligations)     obligation.encode(output,indenter);
    out.println(indent + "</Obligations>");
    indenter.out();
  }
}
