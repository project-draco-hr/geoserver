{
  idAttr=id;
  this.combiningAlg=combiningAlg;
  this.description=description;
  this.target=target;
  this.defaultVersion=defaultVersion;
  if (version == null)   this.version="1.0";
 else   this.version=version;
  metaData=null;
  if (obligations == null)   this.obligations=Collections.emptySet();
 else   this.obligations=Collections.unmodifiableSet(new HashSet<Obligation>(obligations));
  if (parameters == null)   this.parameters=Collections.emptyList();
 else   this.parameters=Collections.unmodifiableList(new ArrayList<CombinerParameter>(parameters));
}
