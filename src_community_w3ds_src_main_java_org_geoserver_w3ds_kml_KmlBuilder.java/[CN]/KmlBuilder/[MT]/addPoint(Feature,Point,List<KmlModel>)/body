{
  Model kmlModel=getKmlModel(feature,point,kmlModels);
  if (kmlModel != null) {
    Placemark placemark=new Placemark().withName(feature.getName().toString());
    placemark.createAndSetLookAt().withLongitude(point.getCoordinate().x).withLatitude(point.getCoordinate().y).withRange(440.8).withTilt(8.3).withHeading(2.7);
    placemark.withGeometry(kmlModel);
    document.addToFeature(placemark);
  }
}
