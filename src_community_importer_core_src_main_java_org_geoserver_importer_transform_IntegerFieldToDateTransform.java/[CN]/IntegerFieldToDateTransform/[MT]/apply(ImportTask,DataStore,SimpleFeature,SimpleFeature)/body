{
  Object val=feature.getAttribute(field);
  Date parsed=null;
  if (val instanceof String) {
    String s=((String)val).trim();
    if (s.length() > 0) {
      val=Double.parseDouble(s);
    }
 else {
      val=null;
    }
  }
  if (val != null) {
    parsed=parseDate((Number)val);
  }
  feature.setAttribute(field,parsed);
  return feature;
}
