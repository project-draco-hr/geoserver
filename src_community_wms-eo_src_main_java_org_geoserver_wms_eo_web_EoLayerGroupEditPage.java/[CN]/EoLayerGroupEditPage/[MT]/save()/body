{
  LayerGroupInfo lg=(LayerGroupInfo)lgModel.getObject();
  getCatalog().save(lg);
  this.extensionPanels.visitChildren(LayerGroupConfigurationPanel.class,new IVisitor<LayerGroupConfigurationPanel>(){
    @Override public Object component(    LayerGroupConfigurationPanel extensionPanel){
      extensionPanel.save();
      return CONTINUE_TRAVERSAL;
    }
  }
);
  doReturn();
}
