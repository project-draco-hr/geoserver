{
  Preconditions.checkArgument(null == sortBy || sortBy.length < 2,"Sort order may only specify a single value");
  SortBy sortOrder=sortBy != null ? sortBy[0] : null;
  Preconditions.checkArgument(null == sortOrder || canSort(of,sortOrder.getPropertyName().getPropertyName()),"Can't sort objects of type %s by %s",of,sortOrder);
  return db.query(of,filter,offset,count,sortOrder);
}
