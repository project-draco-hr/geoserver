{
  FeatureDiffReader clone=null;
  try {
    clone=new FeatureDiffReaderImpl((FeatureDiffReaderImpl)reader);
    int size=0;
    while (clone.hasNext())     size++;
    return size;
  }
 catch (  Exception e) {
    close(reader);
    throw new RuntimeException(e);
  }
 finally {
    if (clone != null) {
      close(clone);
    }
  }
}
