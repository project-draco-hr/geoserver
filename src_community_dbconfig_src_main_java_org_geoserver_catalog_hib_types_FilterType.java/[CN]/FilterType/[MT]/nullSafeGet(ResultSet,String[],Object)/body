{
  String xml=rs.getString(names[0]);
  if (xml == null) {
    return null;
  }
  Parser parser=new Parser(ogc);
  try {
    Filter filter=(Filter)parser.parse(new StringReader(xml));
    return filter;
  }
 catch (  Exception e) {
    String msg="Could not decode filter: " + xml;
    throw new HibernateException(msg,e);
  }
}
