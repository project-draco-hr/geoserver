{
  File props=new File("test.properties");
  if (!props.canRead()) {
    LOGGER.log(Level.INFO,"Cannot read {0}, skipping",props);
    return;
  }
  FileInputStream fis=new FileInputStream(props);
  try {
    m_properties=new Properties();
    m_properties.load(fis);
  }
  finally {
    fis.close();
  }
}
