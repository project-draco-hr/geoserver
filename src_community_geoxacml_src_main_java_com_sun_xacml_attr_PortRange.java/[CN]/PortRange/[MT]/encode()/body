{
  if (isUnbound())   return "";
  if (isSinglePort())   return String.valueOf(lowerBound);
  if (!isLowerBounded())   return "-" + String.valueOf(upperBound);
  if (!isUpperBounded())   return String.valueOf(lowerBound) + "-";
  return String.valueOf(lowerBound) + "-" + String.valueOf(upperBound);
}
