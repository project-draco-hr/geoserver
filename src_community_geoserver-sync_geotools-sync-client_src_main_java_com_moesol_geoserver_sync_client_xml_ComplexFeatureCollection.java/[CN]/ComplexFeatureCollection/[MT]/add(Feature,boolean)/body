{
  if (feature == null)   return false;
  final String ID=feature.getIdentifier().getID();
  if (ID == null)   return false;
  if (contents.containsKey(ID))   return false;
  if (schema == null) {
    schema=feature.getType();
  }
  FeatureType childType=getSchema();
  if (!feature.getType().equals(childType))   LOGGER.warning("Feature Collection contains a heterogeneous" + " mix of features");
  contents.put(ID,feature);
  if (fire) {
    fireChange(feature,CollectionEvent.FEATURES_ADDED);
  }
  return true;
}
