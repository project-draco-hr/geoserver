{
  LOGGER.info(value);
  if (value.equals("-???")) {
    return new Double(Double.NEGATIVE_INFINITY);
  }
 else   if (value.equals("???")) {
    return new Double(Double.POSITIVE_INFINITY);
  }
 else {
    final Number number=parse(value,BigDecimal.valueOf(Double.NEGATIVE_INFINITY),BigDecimal.valueOf(Double.POSITIVE_INFINITY),Locale.US);
    if (number == null) {
      return null;
    }
    return new Double(number.doubleValue());
  }
}
