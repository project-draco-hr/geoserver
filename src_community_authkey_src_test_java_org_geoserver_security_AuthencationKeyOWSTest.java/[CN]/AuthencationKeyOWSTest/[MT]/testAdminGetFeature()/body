{
  Document doc=getAsDOM("wfs?service=WFS&version=1.0.0&request=GetFeature&typeName=" + getLayerId(MockData.PONDS) + "&authkey="+ adminKey);
  assertXpathEvaluatesTo("1","count(//wfs:FeatureCollection)",doc);
  XpathEngine engine=XMLUnit.newXpathEngine();
  String url=engine.evaluate("//wfs:FeatureCollection/@xsi:schemaLocation",doc);
  assertTrue(url.contains("&authkey=" + adminKey));
}
