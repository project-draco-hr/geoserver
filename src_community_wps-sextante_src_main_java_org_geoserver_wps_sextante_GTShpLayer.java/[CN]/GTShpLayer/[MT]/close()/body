{
  if (m_featWriter != null) {
    try {
      m_featWriter.close();
      m_featWriter=null;
    }
 catch (    IOException e) {
      e.printStackTrace();
      Sextante.addErrorToLog(e);
    }
  }
  if (this.getFeatureSource() != null) {
    this.getFeatureSource().getDataStore().dispose();
  }
}
