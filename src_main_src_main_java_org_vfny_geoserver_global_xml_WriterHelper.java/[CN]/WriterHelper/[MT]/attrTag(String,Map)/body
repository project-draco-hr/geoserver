{
  StringBuffer sb=new StringBuffer();
  sb.append("<" + tagName + " ");
  Iterator i=attributes.keySet().iterator();
  while (i.hasNext()) {
    String s=(String)i.next();
    if (attributes.get(s) != null) {
      sb.append(s + " = " + "\""+ escape((attributes.get(s)).toString())+ "\" ");
    }
  }
  sb.append("/>");
  writeln(sb.toString());
}
