{
  getInsert().setString(1,name);
  getInsert().setInt(2,parent);
  getInsert().setBytes(3,content);
  getInsert().execute();
  ResultSet rs=getInsert().getGeneratedKeys();
  if (rs.next()) {
    return rs.getInt(1);
  }
 else {
    throw new IllegalStateException("Could not add test file " + name);
  }
}
