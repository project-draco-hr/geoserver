{
  if (!wfs.getServiceLevel().contains(WFSInfo.ServiceLevel.TRANSACTIONAL)) {
    throw new WFSException("Transaction support is not enabled");
  }
  try {
    return execute(request);
  }
 catch (  WFSException e) {
    abort(request);
    throw e;
  }
catch (  Throwable t) {
    abort(request);
    throw new WFSException(t);
  }
}
