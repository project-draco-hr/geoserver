{
  String xml="<wfs:GetFeature " + "service=\"WFS\" " + "outputFormat=\"SyncChecksum\" "+ "version=\"1.1.0\" "+ "xmlns:cdf=\"http://www.opengis.net/cite/data\" "+ "xmlns:ogc=\"http://www.opengis.net/ogc\" "+ "xmlns:wfs=\"http://www.opengis.net/wfs\" "+ "> "+ "<wfs:Query typeName=\"cite:Buildings\"> "+ "<ogc:Filter>"+ "<ogc:PropertyIsEqualTo> "+ "<ogc:Function name=\"sha1Sync\"> "+ "</ogc:Function> "+ "<ogc:Literal>true</ogc:Literal> "+ "</ogc:PropertyIsEqualTo> "+ "</ogc:Filter> "+ "</wfs:Query> "+ "</wfs:GetFeature>";
  String out=postAsServletResponse("wfs",xml).getOutputStreamContent();
  assertTrue(out,out.contains("Unable to find function sha1Sync"));
}
