{
  GeoServerPasswordEncoder from=(GeoServerPasswordEncoder)service.getSecurityManager().loadPasswordEncoder(service.getPasswordEncoderName());
  if (from.getEncodingType() == PasswordEncodingType.PLAIN || from.getEncodingType() == PasswordEncodingType.ENCRYPT)   return true;
  return service.getPasswordEncoderName().equals(store.getPasswordEncoderName());
}
