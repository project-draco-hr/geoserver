{
  File f=new File("target/fileModelTest.xml");
  try {
    f.createNewFile();
    FileModel model=new FileModel(new Model<String>());
    model.setObject(f.getAbsolutePath());
    String path=(String)model.getObject();
    assertEquals("file://" + f.getAbsolutePath(),path);
  }
  finally {
    f.delete();
  }
}
