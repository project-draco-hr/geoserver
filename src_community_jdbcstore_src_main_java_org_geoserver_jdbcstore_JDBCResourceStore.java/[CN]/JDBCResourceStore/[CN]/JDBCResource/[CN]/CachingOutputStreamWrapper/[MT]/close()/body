{
  final Lock lock=lock();
  try {
    List<ResourceNotification.Event> events=SimpleResourceWatcher.createEvents(JDBCResource.this,ResourceNotification.Kind.ENTRY_MODIFY);
    entry.setContent(new FileInputStream(tempFile));
    resourceWatcher.changed(new ResourceNotification(path(),ResourceNotification.Kind.ENTRY_MODIFY,System.currentTimeMillis(),events));
  }
  finally {
    lock.release();
  }
}
