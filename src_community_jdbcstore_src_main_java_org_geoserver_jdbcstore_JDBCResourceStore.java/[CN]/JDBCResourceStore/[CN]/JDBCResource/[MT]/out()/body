{
  List<ResourceNotification.Event> events=SimpleResourceWatcher.createEvents(this,ResourceNotification.Kind.ENTRY_CREATE);
  if (entry.createResource()) {
    resourceWatcher.changed(new ResourceNotification(path(),ResourceNotification.Kind.ENTRY_CREATE,System.currentTimeMillis(),events));
  }
  try {
    return new CachingOutputStreamWrapper(File.createTempFile("out.",entry.getName()));
  }
 catch (  IOException ex) {
    throw new IllegalStateException(ex);
  }
}
