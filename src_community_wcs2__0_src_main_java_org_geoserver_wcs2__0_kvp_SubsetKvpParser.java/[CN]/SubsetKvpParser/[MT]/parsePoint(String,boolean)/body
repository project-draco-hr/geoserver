{
  point=point.trim();
  if ("*".equals(point)) {
    if (allowStar) {
      return null;
    }
 else {
      throw new OWS20Exception("Invalid usage of *, it can be used only when specifying an interval",WCSExceptionCode.InvalidEncodingSyntax,"subset");
    }
  }
 else   if (point.startsWith("\"") && point.endsWith("\"")) {
    point=point.substring(1,point.length() - 1);
  }
 else {
    try {
      Double.parseDouble(point);
    }
 catch (    NumberFormatException e) {
      throw new OWS20Exception("Invalid point value " + point + ", it is not a number and it's not between double quotes",WCSExceptionCode.InvalidEncodingSyntax,"subset");
    }
  }
  return point;
}
