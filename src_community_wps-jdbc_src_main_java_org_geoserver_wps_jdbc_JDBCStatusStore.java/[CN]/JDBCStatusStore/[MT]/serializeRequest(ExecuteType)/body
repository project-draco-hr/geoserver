{
  String ret="";
  ByteArrayOutputStream out=new ByteArrayOutputStream();
  Encoder e=new Encoder(new WPSConfiguration());
  e.setIndenting(true);
  try {
    e.encode(request,WPS.Execute,out);
    ret=out.toString();
  }
 catch (  IOException e1) {
    LOGGER.info("Problem encountered encoding WPS Request");
  }
  return ret;
}
