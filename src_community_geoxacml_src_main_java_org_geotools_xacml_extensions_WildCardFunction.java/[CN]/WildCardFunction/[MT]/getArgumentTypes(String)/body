{
  if (functionName.equals(NAME_STRING_WILDCARD_MATCH))   return stringWildcardParams;
 else   if (functionName.equals(NAME_ANYURI_WILDCARD_MATCH))   return anyURIWildcardParams;
 else   if (functionName.equals(NAME_IPADDRESS_WILDCARD_MATCH))   return ipAddressWildcardParams;
 else   if (functionName.equals(NAME_DNSNAME_WILDCARD_MATCH))   return dnsNameWildcardParams;
 else   if (functionName.equals(NAME_RFC822NAME_WILDCARD_MATCH))   return rfc822NameWildcardParams;
 else   if (functionName.equals(NAME_X500NAME_WILDCARD_MATCH))   return x500NameWildcardParams;
  return null;
}
