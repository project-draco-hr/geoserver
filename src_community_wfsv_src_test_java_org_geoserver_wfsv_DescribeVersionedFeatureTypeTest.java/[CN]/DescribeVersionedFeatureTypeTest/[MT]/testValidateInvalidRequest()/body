{
  String request="<DescribeVersionedFeatureType\r\n" + "  version=\"1.0.0\"\r\n" + "  service=\"WFSV\" versioned=\"true\"\r\n"+ "  xmlns=\"http://www.opengis.net/wfsv\"\r\n"+ "  xmlns:wfs=\"http://www.opengis.net/wfs\"\r\n"+ "  xmlns:wfsv=\"http://www.opengis.net/wfsv\"\r\n"+ "  xmlns:topp=\"http://www.openplans.org/topp\"\r\n>\r\n"+ "    <wfsv:InvalidElement>topp:archsites</wfsv:InvalidElement>\r\n"+ "</DescribeVersionedFeatureType>";
  Document dom=postAsDOM(root() + "strict=true",request);
  assertEquals("ServiceExceptionReport",dom.getDocumentElement().getNodeName());
}
