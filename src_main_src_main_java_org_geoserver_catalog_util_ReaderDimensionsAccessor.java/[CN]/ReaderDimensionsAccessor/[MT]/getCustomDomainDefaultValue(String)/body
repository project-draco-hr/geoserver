{
  Utilities.ensureNonNull("name",name);
  String minimum=reader.getMetadataValue(name.toUpperCase() + "_DOMAIN_MINIMUM");
  if (minimum != null) {
    return minimum;
  }
  TreeSet<String> domain=getDomain(name);
  if (domain.isEmpty()) {
    return null;
  }
 else {
    return domain.first();
  }
}
