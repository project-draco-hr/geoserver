{
  List<GeoServerPasswordEncoder> encoders=GeoServerExtensions.extensions(GeoServerPasswordEncoder.class);
  boolean found=false;
  for (  GeoServerPasswordEncoder enc : encoders) {
    if (enc.getPrefix() != null && enc.getPrefix().equals("plain4711")) {
      found=true;
      break;
    }
  }
  assertTrue(found);
}
