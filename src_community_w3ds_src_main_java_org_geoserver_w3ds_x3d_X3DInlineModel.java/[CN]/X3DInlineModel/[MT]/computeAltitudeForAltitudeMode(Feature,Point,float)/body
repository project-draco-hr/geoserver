{
  String altitudeModelText=styleFilter.getExpressionValue(model.getAltitudeModel(),feature);
  if (altitudeModelText == null) {
    return 0;
  }
  int altitudeModel=ModelImpl.decodeAltitudeModel(altitudeModelText);
switch (altitudeModel) {
case 0:
    return altitude - point.getCoordinate().Z;
case 1:
  return 0;
case 2:
return 0;
case 3:
return altitude;
case 4:
return altitude;
default :
return 0;
}
}
