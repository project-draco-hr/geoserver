{
  X3DNode modelInline=new X3DNode("Inline");
  X3DNode modelPosition=new X3DNode("Transform");
  modelPosition.addX3DNode(modelInline);
  modelInline.addX3DAttribute("url",styleFilter.getExpressionValue(model.getHref(),feature));
  Coordinate coordinate=point.getCoordinate();
  modelPosition.addX3DAttribute("translation",coordinate.y + " " + coordinate.z+ " "+ coordinate.x);
  return modelPosition;
}
