{
  URL url=AbstractSecurityServiceTest.class.getResource("/datadir");
  if (!"file".equals(url.getProtocol())) {
    File dataDir=File.createTempFile("data","live",new File("./target"));
    dataDir.delete();
    dataDir.mkdirs();
    FileUtils.copyURLToFile(AbstractSecurityServiceTest.class.getResource("/datadir/dummy.txt"),new File(dataDir,"dummy.txt"));
    return dataDir;
  }
  return DataUtilities.urlToFile(url);
}
