{
  BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
  while (true) {
    System.out.print("? ");
    System.out.flush();
    while (System.in.available() < 1) {
      Thread.sleep(100);
      continue;
    }
    String line=br.readLine();
    if (line.length() < 1) {
      continue;
    }
    char c=line.charAt(0);
    try {
      processCommand(c);
    }
 catch (    Exception e) {
      e.printStackTrace(System.err);
    }
  }
}
