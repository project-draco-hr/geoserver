{
  catalog=createNiceMock(Catalog.class);
  expect(catalog.getWorkspace((String)anyObject())).andReturn(createNiceMock(WorkspaceInfo.class)).anyTimes();
  replay(catalog);
  rwUser=new TestingAuthenticationToken("rw","supersecret",new GrantedAuthority[]{new GrantedAuthorityImpl("READER"),new GrantedAuthorityImpl("WRITER")});
