{
  if (keyName.contains("Color"))   return new ColorPickerPanel(id,new MapModel(paramsModel,keyName),new org.apache.wicket.model.ResourceModel(keyName,keyName),false);
 else   return new TextParamPanel(id,new MapModel(paramsModel,keyName),new org.apache.wicket.model.ResourceModel(keyName,keyName),false);
}
