{
  if (response.getRecords() != null) {
    try {
      response.getRecords().accepts(new FeatureVisitor(){
        @Override public void visit(        Feature feature){
          encode(response,feature);
        }
      }
,null);
    }
 catch (    IOException e) {
      throw new ServiceException("Failed to encoder records",e);
    }
  }
}
