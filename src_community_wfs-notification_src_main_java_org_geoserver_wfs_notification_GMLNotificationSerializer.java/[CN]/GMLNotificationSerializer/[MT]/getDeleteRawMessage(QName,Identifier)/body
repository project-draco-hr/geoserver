{
  StringBuilder builder=new StringBuilder(1024);
  builder.append("<wfs:Delete xmlns:wfs=\"http://www.opengis.net/wfs\" xmlns:").append(typeName.getPrefix()).append("=\"").append(typeName.getNamespaceURI()).append("\" typeName=\"").append(typeName.getPrefix()).append(":").append(typeName.getLocalPart()).append("\">").append(id.getID()).append("</wfs:Delete>");
  return builder.toString();
}
