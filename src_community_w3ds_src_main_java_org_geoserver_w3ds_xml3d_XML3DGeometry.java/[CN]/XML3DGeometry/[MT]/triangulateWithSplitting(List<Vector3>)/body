{
  if (vertices.size() < 63000) {
    triangulate(vertexList);
  }
 else {
    if (nodeList == null) {
      nodeList=new ArrayList<XML3DNode>();
    }
    if (LOGGER.isLoggable(Level.FINE))     LOGGER.fine("Add new node to splitted geometry: " + nodeList.size());
    nodeList.add(createXML3DNode());
    triangulate(vertexList);
  }
}
