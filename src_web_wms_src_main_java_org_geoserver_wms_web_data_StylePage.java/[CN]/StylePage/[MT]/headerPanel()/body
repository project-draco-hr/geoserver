{
  Fragment header=new Fragment(HEADER_PANEL,"header",this);
  header.add(new BookmarkablePageLink("addNew",StyleNewPage.class));
  header.add(removal=new SelectionRemovalLink("removeSelected",table,dialog){
    @Override protected StringResourceModel canRemove(    CatalogInfo object){
      StyleInfo s=(StyleInfo)object;
      if (StyleInfo.DEFAULT_POINT.equals(s.getName()) || StyleInfo.DEFAULT_LINE.equals(s.getName()) || StyleInfo.DEFAULT_POLYGON.equals(s.getName())|| StyleInfo.DEFAULT_RASTER.equals(s.getName())) {
        return new StringResourceModel("cantRemoveDefaultStyle",StylePage.this,null);
      }
      return null;
    }
  }
);
  removal.setOutputMarkupId(true);
  removal.setEnabled(false);
  return header;
}
