{
  super(Arrays.asList("png","jpeg","JPEG","PNG"),new HashMap<String,String>(){
{
      put("png","png");
      put("jpeg","jpeg");
      put("JPEG","jpeg");
      put("PNG","png");
      put("image/png","png");
      put("image/jpeg","jpeg");
    }
  }
,new HashMap<String,String>(){
{
      put("png","image/png");
      put("jpeg","image/jpeg");
      put("PNG","image/png");
      put("JPEG","image/jpeg");
    }
  }
);
}
