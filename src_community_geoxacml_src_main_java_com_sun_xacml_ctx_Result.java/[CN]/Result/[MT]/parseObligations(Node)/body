{
  Set<Obligation> set=new HashSet<Obligation>();
  NodeList nodes=root.getChildNodes();
  for (int i=0; i < nodes.getLength(); i++) {
    Node node=nodes.item(i);
    if (node.getNodeName().equals("Obligation"))     set.add(Obligation.getInstance(node));
  }
  if (set.size() == 0)   throw new ParsingException("ObligationsType must not be empty");
  return set;
}
