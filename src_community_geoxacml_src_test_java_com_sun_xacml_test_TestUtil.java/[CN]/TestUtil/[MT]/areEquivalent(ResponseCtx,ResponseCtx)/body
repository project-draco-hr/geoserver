{
  Set<Result> results1=response1.getResults();
  Set<Result> results2=response2.getResults();
  if (results1.size() != results2.size())   return false;
  Iterator<Result> it1=results1.iterator();
  Set<Result> set2=new HashSet<Result>(results2);
  while (it1.hasNext()) {
    Result result1=it1.next();
    Iterator<Result> it2=set2.iterator();
    boolean matched=false;
    while (it2.hasNext() && (!matched)) {
      Result result2=it2.next();
      if ((result1.getDecision() == result2.getDecision()) && (equals(result1.getResource(),result2.getResource())) && areEquivalent(result1.getStatus(),result2.getStatus())&& areEquivalent(result1.getObligations(),result2.getObligations())) {
        matched=true;
      }
    }
    if (matched)     it2.remove();
 else     return false;
  }
  return true;
}
