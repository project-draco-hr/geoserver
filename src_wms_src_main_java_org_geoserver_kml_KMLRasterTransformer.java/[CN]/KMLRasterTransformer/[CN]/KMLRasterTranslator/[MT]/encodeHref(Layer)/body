{
  if (inline) {
    int mapLayerOrder=mapContext.layers().indexOf(layer);
    element("href","images/layer_" + mapLayerOrder + ".png");
  }
 else {
    element("href",WMSRequests.getGetMapUrl(mapContext.getRequest(),layer,0,mapContext.getAreaOfInterest(),new String[]{"format","image/png","transparent","true"}));
  }
}
