{
  if (action == null) {
    return FtpletResult.DEFAULT;
  }
switch (action) {
case CONTINUE:
    return FtpletResult.DEFAULT;
case DISCONNECT:
  return FtpletResult.DISCONNECT;
case SKIP:
return FtpletResult.SKIP;
default :
throw new IllegalArgumentException("Unknown FTP Callback action: " + action);
}
}
