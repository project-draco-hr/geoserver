{
  super.setUp();
  sessionFactory=(SessionFactory)ctx.getBean("hibSessionFactory");
  HibUtil.setUpSession(sessionFactory,true);
  CatalogFacade dao=catalog.getFacade();
  for (  LayerGroupInfo lg : dao.getLayerGroups()) {
    dao.remove(lg);
  }
  for (  LayerInfo l : dao.getLayers()) {
    dao.remove(l);
  }
  for (  ResourceInfo r : dao.getResources(ResourceInfo.class)) {
    dao.remove(r);
  }
  for (  StoreInfo s : dao.getStores(StoreInfo.class)) {
    dao.remove(s);
  }
  for (  WorkspaceInfo ws : dao.getWorkspaces()) {
    dao.remove(ws);
  }
  for (  NamespaceInfo ns : dao.getNamespaces()) {
    dao.remove(ns);
  }
  for (  StyleInfo s : dao.getStyles()) {
    dao.remove(s);
  }
}
