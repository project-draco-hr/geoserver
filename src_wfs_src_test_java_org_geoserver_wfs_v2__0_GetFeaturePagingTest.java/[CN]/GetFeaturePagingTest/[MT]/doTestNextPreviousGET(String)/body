{
  Document doc=getAsDOM("/wfs?request=GetFeature&version=2.0.0&service=wfs&" + "typename=" + typeName + "&count=5");
  assertFalse(doc.getDocumentElement().hasAttribute("previous"));
  assertStartIndexCount(doc,"next",5,5);
  doc=getAsDOM("/wfs?request=GetFeature&version=2.0.0&service=wfs&" + "typename=" + typeName + "&startIndex=5&count=7");
  assertStartIndexCount(doc,"previous",0,5);
  assertStartIndexCount(doc,"next",12,7);
  doc=getAsDOM("/wfs?request=GetFeature&version=2.0.0&service=wfs&" + "typename=" + typeName + "&startIndex=12&count=7");
  assertStartIndexCount(doc,"previous",5,7);
  assertFalse(doc.getDocumentElement().hasAttribute("next"));
  doc=getAsDOM("/wfs?request=GetFeature&version=2.0.0&service=wfs&" + "typename=" + typeName + "&startIndex=15");
  assertStartIndexCount(doc,"previous",0,15);
  assertFalse(doc.getDocumentElement().hasAttribute("next"));
}
