{
  try {
    for (    String serviceName : securityManager.listUserGroupServices()) {
      if (securityManager.loadUserGroupService(serviceName).getUserByUsername(username) != null) {
        return true;
      }
    }
  }
 catch (  IOException e) {
    logger.log(Level.WARNING,e.getMessage(),e);
  }
  return false;
}
