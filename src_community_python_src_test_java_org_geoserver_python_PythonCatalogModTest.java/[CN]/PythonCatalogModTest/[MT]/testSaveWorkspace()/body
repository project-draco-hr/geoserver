{
  testAddWorkspace();
  assertNotNull(cat.getWorkspaceByName("foo"));
  pi.exec("from geoserver.catalog import Workspace");
  pi.exec("ws = Workspace('foo')");
  pi.exec("ws.name = 'bar'");
  pi.exec("ws.save()");
  assertNull(cat.getWorkspaceByName("foo"));
  assertNotNull(cat.getWorkspaceByName("bar"));
}
