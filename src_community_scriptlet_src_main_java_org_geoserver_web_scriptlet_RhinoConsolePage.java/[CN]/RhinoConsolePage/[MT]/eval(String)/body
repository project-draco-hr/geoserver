{
  Context cx=Context.enter();
  Result res=new Result();
  res.input=js;
  try {
    res.response=(String)cx.jsToJava(cx.evaluateString(scope,js,"<input>",0,null),String.class);
  }
 catch (  WrappedException e) {
    res.success=false;
    res.response=e.getMessage();
  }
  Context.exit();
  return res;
}
