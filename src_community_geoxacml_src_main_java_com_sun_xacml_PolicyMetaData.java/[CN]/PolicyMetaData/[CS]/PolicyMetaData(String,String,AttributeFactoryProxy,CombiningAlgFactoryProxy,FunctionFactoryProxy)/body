{
  if (xacmlVersion == null)   this.xacmlVersion=XACML_DEFAULT_VERSION;
 else   if (xacmlVersion.equals(XACML_1_0_IDENTIFIER))   this.xacmlVersion=XACML_VERSION_1_0;
 else   if (xacmlVersion.equals(XACML_2_0_IDENTIFIER))   this.xacmlVersion=XACML_VERSION_2_0;
 else   throw new IllegalArgumentException("Unknown XACML version " + "string: " + xacmlVersion);
  if (xpathVersion != null) {
    if (!xpathVersion.equals(XPATH_1_0_IDENTIFIER))     throw new IllegalArgumentException("Unsupported XPath " + " version: " + xpathVersion);
    this.xpathVersion=XPATH_VERSION_1_0;
  }
 else {
    this.xpathVersion=XPATH_VERSION_UNSPECIFIED;
  }
  proxySetup(attributeFactoryProxy,combiningAlgFactoryProxy,functionFactoryProxy);
}
