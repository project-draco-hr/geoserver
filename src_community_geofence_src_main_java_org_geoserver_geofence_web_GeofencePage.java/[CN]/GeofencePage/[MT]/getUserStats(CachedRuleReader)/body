{
  CacheStats stats;
  StringBuilder sb;
  stats=cacheRuleReader.getUserStats();
  sb=new StringBuilder().append("<b>UserStats</b><ul>").append("<li>size: ").append(cacheRuleReader.getUserCacheSize()).append("/").append(cacheRuleReader.getCacheInitParams().getSize()).append("</li>").append("<li>hitCount: ").append(stats.hitCount()).append("</li>").append("<li>missCount: ").append(stats.missCount()).append("</li>").append("<li>loadSuccessCount: ").append(stats.loadSuccessCount()).append("</li>").append("<li>loadExceptionCount: ").append(stats.loadExceptionCount()).append("</li>").append("<li>totalLoadTime: ").append(stats.totalLoadTime()).append("</li>").append("<li>evictionCount: ").append(stats.evictionCount()).append("</li>").append("</ul>");
  return sb.toString();
}
