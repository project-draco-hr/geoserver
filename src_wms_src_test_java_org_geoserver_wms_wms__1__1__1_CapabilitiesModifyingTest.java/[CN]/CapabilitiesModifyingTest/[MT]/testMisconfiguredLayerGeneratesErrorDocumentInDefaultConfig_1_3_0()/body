{
  for (  FeatureTypeInfo ft : getCatalog().getFeatureTypes()) {
    ft.setLatLonBoundingBox(null);
    getCatalog().save(ft);
  }
  MockHttpServletResponse response=getAsServletResponse("wms?service=WMS&request=GetCapabilities&version=1.3.0");
  assertTrue("Response does not contain ServiceExceptionReport: " + response.getOutputStreamContent(),response.getOutputStreamContent().endsWith("</ServiceExceptionReport>"));
}
