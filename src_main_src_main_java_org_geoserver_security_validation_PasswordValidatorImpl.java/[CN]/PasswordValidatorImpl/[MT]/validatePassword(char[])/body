{
  if (password == null) {
    password=new char[]{};
  }
  if (password.length < config.getMinLength())   throw createSecurityException(PW_MIN_LENGTH,config.getMinLength());
  if (config.getMaxLength() >= 0 && password.length > config.getMaxLength())   throw createSecurityException(PW_MAX_LENGTH,config.getMaxLength());
  if (config.isDigitRequired()) {
    if (checkUsingMethod("isDigit",password) == false)     throw createSecurityException(PW_NO_DIGIT);
  }
  if (config.isUppercaseRequired()) {
    if (checkUsingMethod("isUpperCase",password) == false)     throw createSecurityException(PW_NO_UPPERCASE);
  }
  if (config.isLowercaseRequired()) {
    if (checkUsingMethod("isLowerCase",password) == false)     throw createSecurityException(PW_NO_LOWERCASE);
  }
  O:   for (  String prefix : notAllowedPrefixes) {
    if (prefix.length() > password.length) {
      continue;
    }
    for (int i=0; i < prefix.length(); i++) {
      if (prefix.charAt(i) != password[i]) {
        continue O;
      }
    }
    throw createSecurityException(PW_RESERVED_PREFIX,prefix);
  }
}
