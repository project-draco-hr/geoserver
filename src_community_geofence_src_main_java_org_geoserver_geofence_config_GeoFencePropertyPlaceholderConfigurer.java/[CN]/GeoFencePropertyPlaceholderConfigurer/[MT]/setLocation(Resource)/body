{
  super.setLocation(location);
  try {
    File f=location.getFile();
    if (f != null && !f.isAbsolute()) {
      f=new File(data.root(),f.getPath());
      location=new UrlResource(f.toURI());
      this.configFile=f;
    }
  }
 catch (  IOException e) {
    LOGGER.log(Level.WARNING,"Error reading resource " + location,e);
  }
}
