{
  final StringBuilder sb=new StringBuilder();
  for (  String filename : this.dataDirectory.list()) {
    if (isDataFile(filename)) {
      sb.append(getDimensionValue(filename));
      sb.append(',');
    }
  }
  final int len=sb.length();
  if (len == 0) {
    return null;
  }
  return sb.substring(0,len - 1);
}
