{
  Object retVal=null;
  if (param instanceof ParameterValue<?>) {
    final Object paramVal=((ParameterValue<?>)param).getValue();
    if (paramVal != null) {
      if (paramVal instanceof List) {
        final List<?> list=(List<?>)paramVal;
        if (!list.isEmpty())         retVal=list.get(0);
      }
 else {
        retVal=paramVal;
      }
    }
  }
  return retVal;
}
