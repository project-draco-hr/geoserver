{
  this.output=output;
  geoSystem=new Attribute("geoSystem","&quot;GD&quot; &quot;WE&quot; &quot;longitude_first&quot;");
  this.origin=new Node("GeoOrigin");
  this.origin.addAttribute(this.geoSystem);
  this.origin.addAttribute(new Attribute("DEF","ORIGIN"));
  this.origin.addAttribute(new Attribute("geoCoords",String.valueOf(origin[0]) + " " + String.valueOf(origin[1])+ " "+ String.valueOf(origin[2])));
}
