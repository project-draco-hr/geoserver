{
  File wpsStore=null;
  try {
    File temp=GeoserverDataDirectory.findCreateConfigDir("temp");
    wpsStore=new File(temp,"wps");
    if (!wpsStore.exists()) {
      mkdir(wpsStore);
    }
  }
 catch (  Exception e) {
    throw new WcsException("Could not create the temporary storage directory for WPS");
  }
  return wpsStore;
}
