{
  if (name == null) {
    throw new NullPointerException("name must be defined to get a PropertyDescriptor.");
  }
  if (pds == null) {
    pds=getPropertyDescriptors();
  }
  for (int i=0; i < pds.length; i++) {
    if (name.equals(pds[i].getName())) {
      return pds[i];
    }
  }
  return null;
}
