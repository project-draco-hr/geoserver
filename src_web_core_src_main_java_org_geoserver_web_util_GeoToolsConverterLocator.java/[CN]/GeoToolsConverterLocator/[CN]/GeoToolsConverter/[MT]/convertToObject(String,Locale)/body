{
  for (  ConverterFactory factory : factories) {
    try {
      Converter converter=factory.createConverter(String.class,target,null);
      if (converter != null) {
        Object converted=converter.convert(value,target);
        if (converted != null) {
          return converted;
        }
      }
    }
 catch (    Exception e) {
    }
  }
  return null;
}
