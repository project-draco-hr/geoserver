{
  doInitialize();
  initializeTester();
  newFormTester();
  form.setValue("username","user");
  form.setValue("password","pwd1");
  form.setValue("confirmPassword","pwd2");
  form.submit("save");
  assertTrue(testErrorMessagesWithRegExp(".*[Pp]assword.*"));
  tester.assertRenderedPage(pageClass);
}
