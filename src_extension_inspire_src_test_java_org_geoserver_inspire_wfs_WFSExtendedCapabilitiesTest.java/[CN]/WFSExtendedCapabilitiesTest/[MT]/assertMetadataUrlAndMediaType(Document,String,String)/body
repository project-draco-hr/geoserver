{
  XpathEngine xpath=getXpathEngine();
  assertEquals("Existence of ExtendedCapabilities element","1",xpath.evaluate("count(//inspire_dls:ExtendedCapabilities)",dom));
  assertEquals("Expected MetadataURL URL",metadataUrl,xpath.evaluate("//inspire_dls:ExtendedCapabilities/inspire_common:MetadataUrl/inspire_common:URL",dom));
  assertEquals("Expected MetadataURL MediaType",metadataMediaType,xpath.evaluate("//inspire_dls:ExtendedCapabilities/inspire_common:MetadataUrl/inspire_common:MediaType",dom));
}
