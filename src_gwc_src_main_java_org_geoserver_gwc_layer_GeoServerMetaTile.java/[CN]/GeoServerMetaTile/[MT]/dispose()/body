{
  if (metaTileMap != null) {
    RenderedImage image=metaTileMap.getImage();
    if (image instanceof PlanarImage) {
      ImageUtilities.disposePlanarImageChain((PlanarImage)image);
    }
 else     if (image instanceof BufferedImage) {
      ((BufferedImage)image).flush();
    }
    metaTileMap.dispose();
    metaTileMap=null;
  }
}
