{
  if (f.isFile()) {
    if (f.getName().endsWith(".xml") || f.getName().endsWith(".XML"))     fileNames.add(f.getAbsolutePath());
  }
  if (f.isDirectory()) {
    File[] children=f.listFiles();
    if (children == null)     return;
    for (    File child : children) {
      collectXMLFiles(child,fileNames);
    }
  }
}
