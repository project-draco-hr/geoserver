{
  int count=0;
  for (Iterator i=getDataStores().iterator(); i.hasNext(); ) {
    DataStoreInfo meta=(DataStoreInfo)i.next();
    if (!meta.isEnabled()) {
      continue;
    }
    try {
      meta.getDataStore(null);
    }
 catch (    Throwable notAvailable) {
      continue;
    }
    count+=1;
  }
  return count;
}
