{
  if (detail != null) {
    String c=(String)(code.iterator().next());
    if (c.equals(STATUS_OK) || c.equals(STATUS_SYNTAX_ERROR) || c.equals(STATUS_PROCESSING_ERROR))     throw new IllegalArgumentException("status detail cannot be " + "included with " + c);
  }
  this.code=Collections.unmodifiableList(new ArrayList<String>(code));
  this.message=message;
  this.detail=detail;
}
