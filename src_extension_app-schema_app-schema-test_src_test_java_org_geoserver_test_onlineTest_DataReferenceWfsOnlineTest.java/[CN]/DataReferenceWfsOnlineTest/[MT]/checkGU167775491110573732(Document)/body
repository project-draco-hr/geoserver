{
  assertXpathEvaluatesTo("typicalNorm","//gsml:GeologicUnit[@gml:id='gsml.geologicunit.167775491110573732']/gsml:purpose",doc);
  assertXpathEvaluatesTo("urn:cgi:classifierScheme:GSV:Lithodemic_IntrusiveUnitRank","//gsml:GeologicUnit[@gml:id='gsml.geologicunit.167775491110573732']/gsml:rank/@codeSpace",doc);
  assertXpathEvaluatesTo("Intrusion [rank]","//gsml:GeologicUnit[@gml:id='gsml.geologicunit.167775491110573732']/gsml:rank",doc);
}
