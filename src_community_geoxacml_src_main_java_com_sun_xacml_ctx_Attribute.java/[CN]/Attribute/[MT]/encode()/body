{
  String encoded="<Attribute AttributeId=\"" + id.toString() + "\" "+ "DataType=\""+ type.toString()+ "\"";
  if (issuer != null)   encoded+=" Issuer=\"" + issuer + "\"";
  if (issueInstant != null)   encoded+=" IssueInstant=\"" + issueInstant.encode() + "\"";
  encoded+=">" + value.encodeWithTags(false) + "</Attribute>";
  return encoded;
}
