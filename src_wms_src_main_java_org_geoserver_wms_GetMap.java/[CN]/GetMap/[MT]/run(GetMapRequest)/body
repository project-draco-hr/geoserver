{
  WMSMapContent mapContent=new WMSMapContent(request);
  try {
    return run(request,mapContent);
  }
 catch (  ServiceException e) {
    mapContent.dispose();
    throw e;
  }
catch (  RuntimeException e) {
    mapContent.dispose();
    throw (RuntimeException)e;
  }
catch (  Exception e) {
    mapContent.dispose();
    throw new ServiceException("Internal error ",e);
  }
}
