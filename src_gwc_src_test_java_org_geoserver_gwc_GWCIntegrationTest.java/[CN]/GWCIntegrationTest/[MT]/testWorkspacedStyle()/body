{
  String layerId=String.format("%s:%s",TEST_WORKSPACE_NAME,WORKSPACED_LAYER);
  MockHttpServletResponse sr=getAsServletResponse("gwc/service/wmts?request=GetTile&layer=" + layerId + "&format=image/png&tilematrixset=EPSG:4326&workspace="+ TEST_WORKSPACE_NAME+ "&tilematrix=EPSG:4326:0&tilerow=0&tilecol=0");
  assertEquals(200,sr.getErrorCode());
  assertEquals("image/png",sr.getContentType());
}
