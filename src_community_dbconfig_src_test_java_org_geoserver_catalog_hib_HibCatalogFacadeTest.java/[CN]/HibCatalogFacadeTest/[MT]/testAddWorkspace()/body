{
  assertEquals(0,dao.getWorkspaces().size());
  WorkspaceInfo ws=dao.getCatalog().getFactory().createWorkspace();
  ws.setName("acme");
  assertNull(ws.getId());
  dao.add(ws);
  assertNotNull(ws.getId());
  assertEquals(1,dao.getWorkspaces().size());
  assertEquals(ws,dao.getWorkspace(ws.getId()));
}
