{
  if (m_attributesToInclude.contains(p.getName().toString())) {
    return true;
  }
  if (m_attributesToInclude.contains("-" + p.getName().toString())) {
    return false;
  }
  if (m_attributesToInclude.contains("-all")) {
    return true;
  }
  if (m_attributesToInclude.contains("*")) {
    return true;
  }
  LOGGER.log(Level.FINER,"Skipping: {0}",p.getName());
  return false;
}
