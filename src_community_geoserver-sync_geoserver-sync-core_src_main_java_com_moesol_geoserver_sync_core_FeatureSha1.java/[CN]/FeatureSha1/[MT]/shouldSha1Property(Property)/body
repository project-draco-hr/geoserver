{
  String localName=p.getName().getLocalPart();
  if (m_attributesToInclude.contains(localName)) {
    return true;
  }
  if (m_attributesToInclude.contains("-" + localName)) {
    return false;
  }
  if (m_attributesToInclude.contains("-all")) {
    return true;
  }
  if (m_attributesToInclude.contains("*")) {
    return true;
  }
  LOGGER.log(Level.FINER,"Skipping: {0}",localName);
  return false;
}
