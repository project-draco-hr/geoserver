{
  formats.add(new SimpleDateFormat(pattern.replaceAll("%","-")));
  formats.add(new SimpleDateFormat(pattern.replaceAll("%","/")));
  formats.add(new SimpleDateFormat(pattern.replaceAll("%",".")));
  formats.add(new SimpleDateFormat(pattern.replaceAll("%"," ")));
  formats.add(new SimpleDateFormat(pattern.replaceAll("%",",")));
}
