{
  FeatureSource fs=layerInfo.getFeatureSource(null,null);
  Query q=new Query();
  q.setMaxFeatures(1);
  FeatureCollection features=fs.getFeatures(q);
  FeatureIterator fi=null;
  Feature sample=null;
  try {
    fi=features.features();
    sample=fi.next();
  }
  finally {
    fi.close();
  }
  return sample;
}
