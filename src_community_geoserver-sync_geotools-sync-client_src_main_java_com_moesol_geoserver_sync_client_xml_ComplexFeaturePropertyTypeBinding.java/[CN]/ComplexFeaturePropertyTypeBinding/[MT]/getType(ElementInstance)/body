{
  AttributeType featureType;
  XSDTypeDefinition def=instance.getTypeDefinition();
  if (def.getName() == null)   featureType=registry.getDescriptor(new NameImpl(instance.getName()),null).getType();
 else   featureType=registry.getAttributeType(new NameImpl(def.getTargetNamespace(),def.getName()));
  return featureType;
}
