{
  StringBuilder sb=new StringBuilder();
  sb.append(kw.getValue());
  if (kw.getLanguage() != null) {
    sb.append("\\@language=").append(kw.getLanguage()).append("\\;");
  }
  if (kw.getVocabulary() != null) {
    sb.append("\\@vocabulary=").append(kw.getVocabulary()).append("\\;");
  }
  return sb.toString();
}
