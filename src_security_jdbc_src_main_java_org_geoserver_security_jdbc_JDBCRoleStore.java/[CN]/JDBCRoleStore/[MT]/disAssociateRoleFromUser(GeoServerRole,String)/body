{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=getConnection();
    ps=getDMLStatement("userroles.delete",con);
    ps.setString(1,role.getAuthority());
    ps.setString(2,username);
    ps.execute();
  }
 catch (  SQLException ex) {
    throw new IOException(ex);
  }
 finally {
    closeFinally(con,ps,null);
  }
  setModified(true);
}
