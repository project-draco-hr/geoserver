{
  final FeatureType featureType=(FeatureType)item.getMetadata().get(FeatureType.class);
  final FeatureIterator it=new FeatureJSON().streamFeatureCollection(file(data,item));
  return new FeatureReader(){
    @Override public FeatureType getFeatureType(){
      return featureType;
    }
    @Override public boolean hasNext() throws IOException {
      return it.hasNext();
    }
    @Override public Feature next() throws IOException, IllegalArgumentException, NoSuchElementException {
      return it.next();
    }
    @Override public void close() throws IOException {
      it.close();
    }
  }
;
}
