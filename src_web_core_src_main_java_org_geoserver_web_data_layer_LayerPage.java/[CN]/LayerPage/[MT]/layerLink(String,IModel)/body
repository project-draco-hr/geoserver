{
  IModel layerNameModel=NAME.getModel(model);
  String wsName=(String)WORKSPACE.getModel(model).getObject();
  String layerName=(String)layerNameModel.getObject();
  return new SimpleBookmarkableLink(id,ResourceConfigurationPage.class,layerNameModel,ResourceConfigurationPage.NAME,layerName,ResourceConfigurationPage.WORKSPACE,wsName);
}
