{
  String rootDir=getItem(workspaceName,storeName,catalog,ROOT_KEY);
  if (rootDir != null) {
    File root=new File(rootDir);
    if (!root.exists()) {
      if (!root.mkdirs()) {
        root.delete();
        return null;
      }
    }
 else {
      if (!root.isDirectory()) {
        LOGGER.info(rootDir + " ROOT path is not a directory");
        return null;
      }
    }
  }
  return rootDir;
}
