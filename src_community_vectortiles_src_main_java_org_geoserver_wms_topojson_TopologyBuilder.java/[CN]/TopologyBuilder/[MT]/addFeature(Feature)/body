{
  String layer=feature.getName().getLocalPart();
  TopoGeom topoObj;
  try {
    topoObj=createObject(feature);
  }
 catch (  MismatchedDimensionException|TransformException e) {
    e.printStackTrace();
    throw Throwables.propagate(e);
  }
  if (topoObj != null) {
    layers.put(layer,topoObj);
  }
}
