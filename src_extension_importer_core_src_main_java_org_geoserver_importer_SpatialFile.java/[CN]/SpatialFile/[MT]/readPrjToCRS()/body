{
  Resource prj=getPrjFile();
  if (prj == null || !Resources.exists(prj)) {
    return null;
  }
  InputStream is=prj.in();
  String wkt=IOUtils.toString(is);
  is.close();
  try {
    return CRS.parseWKT(wkt);
  }
 catch (  Exception e) {
    throw (IOException)new IOException().initCause(e);
  }
}
