{
  getGeneralFactory();
  FunctionFactory newGeneral=new BaseFunctionFactory(generalFunctions,generalAbstractFunctions);
  FunctionFactory newCondition=new BaseFunctionFactory(newGeneral,conditionFunctions,conditionAbstractFunctions);
  FunctionFactory newTarget=new BaseFunctionFactory(newCondition,targetFunctions,targetAbstractFunctions);
  return new BasicFunctionFactoryProxy(newTarget,newCondition,newGeneral);
}
