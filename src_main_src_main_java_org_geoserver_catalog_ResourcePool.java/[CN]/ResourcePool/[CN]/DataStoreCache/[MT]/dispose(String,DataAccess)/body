{
  DataStoreInfo info=catalog.getDataStore(id);
  String name=null;
  if (info != null) {
    name=info.getName();
    LOGGER.info("Disposing datastore '" + name + "'");
    fireDisposed(info,da);
  }
  try {
    da.dispose();
  }
 catch (  Exception e) {
    LOGGER.warning("Error occured disposing datastore '" + name + "'");
    LOGGER.log(Level.FINE,"",e);
  }
}
