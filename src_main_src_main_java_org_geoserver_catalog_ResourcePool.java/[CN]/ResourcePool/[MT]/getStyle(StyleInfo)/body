{
  Style style=styleCache.get(info);
  if (style == null) {
synchronized (styleCache) {
      style=styleCache.get(info);
      if (style == null) {
        style=dataDir().parsedStyle(info,true);
        style.setName(info.getName());
        styleCache.put(info,style);
      }
    }
  }
  return style;
}
