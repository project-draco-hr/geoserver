{
  if (LOGGER.isLoggable(Level.FINE)) {
    LOGGER.fine("AuthenticationCache removing all entries");
    LOGGER.fine("Cache entries #: " + cache.size());
  }
  cache.invalidateAll();
  if (LOGGER.isLoggable(Level.FINE)) {
    LOGGER.fine("AuthenticationCache removed all entries");
    LOGGER.fine("Cache entries #: " + cache.size());
  }
}
