{
  File f=file(data);
  if (f == null) {
    return null;
  }
  CatalogBuilder cb=new CatalogBuilder(catalog);
  cb.setWorkspace(workspace);
  CoverageStoreInfo store=cb.buildCoverageStore(data.getName());
  store.setURL(f.toURI().toURL().toString());
  store.setType(gridFormat().getName());
  return store;
}
