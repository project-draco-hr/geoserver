{
  if ((target != SUBJECT_TARGET) && (target != RESOURCE_TARGET) && (target != ACTION_TARGET)&& (target != ENVIRONMENT_TARGET))   throw new IllegalArgumentException("Input target is not a valid" + "value");
  this.target=target;
  this.type=type;
  this.id=id;
  this.mustBePresent=mustBePresent;
  this.issuer=issuer;
  subjectCategory=null;
}
