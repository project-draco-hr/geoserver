{
  Document dom=getAsDOM("/geosearch/sitemap.xml");
  assertXpathExists("/sm:urlset",dom);
  assertXpathEvaluatesTo(String.valueOf(indexed.length),"count(/sm:urlset/sm:url/sm:loc)",dom);
  assertXpathEvaluatesTo(String.valueOf(indexed.length),"count(/sm:urlset/sm:url/geo:geo/geo:format)",dom);
  assertXpathEvaluatesTo("kml","/sm:urlset/sm:url/geo:geo/geo:format",dom);
}
