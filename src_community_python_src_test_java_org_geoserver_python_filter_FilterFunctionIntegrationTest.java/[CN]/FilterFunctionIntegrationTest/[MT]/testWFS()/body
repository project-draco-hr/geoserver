  String xml="<wfs:GetFeature " + "service=\"WFS\" version=\"1.1.0\" " + "xmlns:sf=\"http://cite.opengeospatial.org/gmlsf\" "+ "xmlns:wfs=\"http://www.opengis.net/wfs\" "+ "xmlns:ogc=\"http://www.opengis.net/ogc\" > "+ "<wfs:Query typeName=\"sf:PrimitiveGeoFeature\"> "+ "<ogc:Filter>"+ "<ogc:PropertyIsEqualTo> "+ "<ogc:Function name=\"myFilter\">"+ "</ogc:Function> "+ "<ogc:Literal>true</ogc:Literal> "+ "</ogc:PropertyIsEqualTo> "+ "</ogc:Filter> "+ "</wfs:Query>"+ "</wfs:GetFeature>";
