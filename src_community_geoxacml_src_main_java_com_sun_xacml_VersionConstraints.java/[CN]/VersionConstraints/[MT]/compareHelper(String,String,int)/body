{
  if (constraint == null)   return true;
  if (version == null)   return true;
  StringTokenizer vtok=new StringTokenizer(version,".");
  StringTokenizer ctok=new StringTokenizer(constraint,".");
  while (vtok.hasMoreTokens()) {
    if (!ctok.hasMoreTokens()) {
      if (type == COMPARE_GREATER)       return true;
 else       return false;
    }
    String c=ctok.nextToken();
    if (c.equals("+"))     return true;
    String v=vtok.nextToken();
    if (!c.equals("*")) {
      if (!v.equals(c)) {
        if (type == COMPARE_EQUAL)         return false;
        int cint=Integer.valueOf(c).intValue();
        int vint=Integer.valueOf(v).intValue();
        if (type == COMPARE_LESS)         return vint <= cint;
 else         return vint >= cint;
      }
    }
  }
  if (ctok.hasMoreTokens()) {
    if (type == COMPARE_LESS)     return true;
 else     return false;
  }
  return true;
}
