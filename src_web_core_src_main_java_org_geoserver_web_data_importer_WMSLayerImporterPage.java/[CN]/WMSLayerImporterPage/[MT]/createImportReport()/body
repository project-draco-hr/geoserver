{
  if (importCount > 0) {
    info("Succesfully imported " + importCount + " layers");
  }
 else {
    info("No new layers were imported");
  }
  if (updateCount > 0) {
    info("Updated " + updateCount + " layers");
  }
  if (errorCount > 0) {
    error("Unable to import " + errorCount + " layers, you may want to import them manually");
  }
}
