{
  Document dom1=getAsDOM("wfs?request=getfeature&service=wfs&version=1.0.0&typename=" + MockData.POLYGONS.getLocalPart());
  Document dom2=getAsDOM("wfs?request=getfeature&service=wfs&version=1.0.0&typename=" + MockData.POLYGONS.getLocalPart() + "&srsName=AUTO:42001,9001,-93,0");
  MathTransform tx=CRS.findMathTransform(CRS.decode("EPSG:32615"),CRS.decode("AUTO:42001,9001,-93,0"));
  runTest(dom1,dom2,tx);
}
