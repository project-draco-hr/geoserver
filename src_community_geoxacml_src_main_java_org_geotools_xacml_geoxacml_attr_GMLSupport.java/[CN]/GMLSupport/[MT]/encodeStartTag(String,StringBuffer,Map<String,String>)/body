{
  buff.append("<").append(GMLNS_PREFIX).append(":").append(elemName);
  if (attributeMap != null) {
    for (    String name : attributeMap.keySet()) {
      buff.append(" ").append(name).append("=\"");
      buff.append(attributeMap.get(name));
      buff.append("\"");
    }
  }
  buff.append(">");
}
