{
  Map<String,String> res=new HashMap<String,String>();
  if (attr.getSrsName() != null)   res.put("srsName",attr.getSrsName());
  if (attr.getGid() != null)   res.put("gid",attr.getGid());
  if (attr.getSrsDimension() > 0)   res.put("srsDimension",new Integer(attr.getSrsDimension()).toString());
  return res;
}
