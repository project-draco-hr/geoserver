{
  PolicyFinderResult finderResult=policyFinder.findPolicy(context);
  if (finderResult.notApplicable())   return new Result(Result.DECISION_NOT_APPLICABLE,context.getResourceId().encode());
  if (finderResult.indeterminate())   return new Result(Result.DECISION_INDETERMINATE,finderResult.getStatus(),context.getResourceId().encode());
  return finderResult.getPolicy().evaluate(context);
}
