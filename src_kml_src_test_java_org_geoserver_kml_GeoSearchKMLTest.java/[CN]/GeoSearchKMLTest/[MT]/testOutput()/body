{
  final String path="wms?request=getmap&service=wms&version=1.1.1" + "&format=" + KMLMapOutputFormat.MIME_TYPE + "&layers="+ MockData.BASIC_POLYGONS.getPrefix()+ ":"+ MockData.BASIC_POLYGONS.getLocalPart()+ "&styles="+ MockData.BASIC_POLYGONS.getLocalPart()+ "&height=1024&width=1024&bbox=-180,-90,0,90&srs=EPSG:4326"+ "&featureid=BasicPolygons.1107531493643";
  Document document=getAsDOM(path);
  assertEquals("kml",document.getDocumentElement().getTagName());
  ;
}
