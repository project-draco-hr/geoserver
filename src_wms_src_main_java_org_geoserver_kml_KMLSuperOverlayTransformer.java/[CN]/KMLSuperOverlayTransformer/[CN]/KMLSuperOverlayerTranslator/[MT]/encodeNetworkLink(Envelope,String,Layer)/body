{
  start("NetworkLink");
  element("name",name);
  encodeRegion(box,128,-1);
  start("Link");
  String getMap=KMLUtils.getMapUrl(mapContent,layer,0,box,new String[]{"format",KMLMapOutputFormat.MIME_TYPE,"width","256","height","256","superoverlay","true"},false,wms.getGeoServer());
  element("href",getMap);
  LOGGER.fine("Network link " + name + ":"+ getMap);
  element("viewRefreshMode","onRegion");
  end("Link");
  end("NetworkLink");
}
