{
  assertXpathCount(7,"//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name",doc);
  assertXpathEvaluatesTo("MERCIA MUDSTONE GROUP","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[1]",doc);
  assertXpathEvaluatesTo("nameone 2","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[2]",doc);
  assertXpathEvaluatesTo("some uri 2","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[2]/@codeSpace",doc);
  assertXpathEvaluatesTo("nameone 3","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[3]",doc);
  assertXpathEvaluatesTo("some uri 3","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[3]/@codeSpace",doc);
  assertXpathEvaluatesTo("nametwo 2","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[4]",doc);
  assertXpathEvaluatesTo("some:uri:mf2","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[4]/@codeSpace",doc);
  assertXpathEvaluatesTo("nametwo 3","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[5]",doc);
  assertXpathEvaluatesTo("some:uri:mf2","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[5]/@codeSpace",doc);
  assertXpathEvaluatesTo("some:uri:2","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[6]/@xlink:href",doc);
  assertXpathEvaluatesTo("some:uri:3","//gsml:MappedFeature[@gml:id='gsml.mappedfeature.mf2']/gml:name[7]/@xlink:href",doc);
}
