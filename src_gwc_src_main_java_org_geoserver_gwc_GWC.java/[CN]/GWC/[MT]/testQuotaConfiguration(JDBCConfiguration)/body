{
  JDBCQuotaStoreFactory factory=GeoServerExtensions.bean(JDBCQuotaStoreFactory.class);
  QuotaStore qs=null;
  try {
    qs=factory.getQuotaStore(applicationContext,"JDBC");
  }
  finally {
    if (qs != null) {
      try {
        qs.close();
      }
 catch (      Exception e) {
        log.log(Level.FINE,"Failed to dispose test quota store",e);
      }
    }
  }
}
