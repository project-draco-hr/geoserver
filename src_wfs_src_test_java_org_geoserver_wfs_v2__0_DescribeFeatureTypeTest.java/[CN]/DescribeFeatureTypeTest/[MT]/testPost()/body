{
  String typeName=getLayerId(MockData.PRIMITIVEGEOFEATURE);
  String xml="<wfs:DescribeFeatureType service='WFS' version='2.0.0' " + "xmlns:wfs='http://www.opengis.net/wfs/2.0' " + "xmlns:sf='" + MockData.PRIMITIVEGEOFEATURE.getNamespaceURI() + "'>"+ " <wfs:TypeName>"+ typeName+ "</wfs:TypeName>"+ "</wfs:DescribeFeatureType>";
  Document doc=postAsDOM("wfs",xml);
  assertSchema(doc,MockData.PRIMITIVEGEOFEATURE);
}
