{
  StringBuilder sb=new StringBuilder();
  if (null != mapContext.getAbstract()) {
    sb.append(mapContext.getAbstract());
  }
  if (null != mapContext.getKeywords()) {
    sb.append("\n");
    for (    String kw : mapContext.getKeywords()) {
      if (null != kw) {
        sb.append(kw).append(" ");
      }
    }
  }
  return sb.toString();
}
