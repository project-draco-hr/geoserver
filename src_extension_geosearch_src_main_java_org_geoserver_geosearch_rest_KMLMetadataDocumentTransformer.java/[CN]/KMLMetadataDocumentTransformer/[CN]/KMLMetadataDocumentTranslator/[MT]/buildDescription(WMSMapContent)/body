{
  StringBuilder sb=new StringBuilder();
  if (null != mapContent.getAbstract()) {
    sb.append(mapContent.getAbstract());
  }
  if (null != mapContent.getKeywords()) {
    sb.append("\n");
    for (    String kw : mapContent.getKeywords()) {
      if (null != kw) {
        sb.append(kw).append(" ");
      }
    }
  }
  return sb.toString();
}
