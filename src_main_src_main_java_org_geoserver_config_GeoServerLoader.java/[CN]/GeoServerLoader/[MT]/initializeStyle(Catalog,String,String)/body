{
  if (resourceLoader.find("styles",sld) == null) {
    FileUtils.copyURLToFile(GeoServerLoader.class.getResource(sld),new File(resourceLoader.findOrCreateDirectory("styles"),sld));
  }
  StyleInfo s=catalog.getFactory().createStyle();
  s.setName(styleName);
  s.setFilename(sld);
  catalog.add(s);
}
