{
  for (  XStreamServiceLoader<ServiceInfo> l : loaders) {
    try {
      ServiceInfo s=l.load(geoServer,directory);
      if (directory != null && s.getWorkspace() == null)       continue;
      geoServer.add(s);
      LOGGER.info("Loaded service '" + s.getId() + "', "+ (s.isEnabled() ? "enabled" : "disabled"));
    }
 catch (    Throwable t) {
      t.printStackTrace();
    }
  }
}
