{
  IModel propertiesModel=HashMapIModel.hashMap(new PropertyModel(model,"resource.metadata.map"),"x3d.objectclass");
  objectClass=new Palette("dclazz",propertiesModel,new AttributeNamesModel(new PropertyModel(model,"resource")),new ColNameRender(),10,false){
    @Override public Component newSelectedHeader(    final String componentId){
      return new Label(componentId,new ResourceModel("X3DLayerConfigPanel.selectedHeader"));
    }
    @Override public Component newAvailableHeader(    final String componentId){
      return new Label(componentId,new ResourceModel("X3DLayerConfigPanel.availableHeader"));
    }
  }
;
  add(objectClass);
}
