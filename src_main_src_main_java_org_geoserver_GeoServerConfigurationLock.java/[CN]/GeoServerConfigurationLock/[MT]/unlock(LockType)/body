{
  if (!enabled) {
    return;
  }
  Lock lock;
  if (type == LockType.WRITE) {
    lock=readWriteLock.writeLock();
  }
 else {
    lock=readWriteLock.readLock();
  }
  if (LOGGER.isLoggable(LEVEL)) {
    LOGGER.log(LEVEL,"Thread " + Thread.currentThread().getId() + " releasing the lock in mode "+ type);
  }
  lock.unlock();
}
