{
  if (inputs.size() < 2)   throw new IllegalArgumentException("not enough args to " + NAME_URI_STRING_CONCATENATE);
  Iterator<? extends Expression> it=inputs.iterator();
  if (!(it.next()).getType().toString().equals(AnyURIAttribute.identifier))   throw new IllegalArgumentException("illegal parameter");
  while (it.hasNext()) {
    if (!((Expression)(it.next())).getType().toString().equals(StringAttribute.identifier))     throw new IllegalArgumentException("illegal parameter");
  }
}
