{
  Node root=getRootNode(testFile);
  NodeList children=root.getChildNodes();
  for (int i=0; i < children.getLength(); i++) {
    Node child=children.item(i);
    String childName=child.getNodeName();
    if (childName.equals("group"))     tests.add(BasicGroupTest.getInstance(child,pdp,policyModule));
 else     if (childName.equals("test"))     tests.add(BasicTest.getInstance(child,pdp,policyModule));
  }
}
