{
  Iterator<Test> it=tests.iterator();
  int errorCount=0;
  System.out.println("STARTING TESTS at " + new Date());
  while (it.hasNext()) {
    Test test=it.next();
    errorCount+=test.run(prefix);
  }
  System.out.println("FINISHED TESTS at " + new Date());
  System.out.println("Total Failed: " + errorCount);
}
