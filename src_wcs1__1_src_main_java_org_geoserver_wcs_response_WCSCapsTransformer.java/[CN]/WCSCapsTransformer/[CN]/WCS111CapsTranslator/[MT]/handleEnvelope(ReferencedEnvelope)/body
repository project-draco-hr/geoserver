{
  start("ows:WGS84BoundingBox");
  element("ows:LowerCorner",new StringBuffer(Double.toString(envelope.getLowerCorner().getOrdinate(0))).append(" ").append(envelope.getLowerCorner().getOrdinate(1)).toString());
  element("ows:UpperCorner",new StringBuffer(Double.toString(envelope.getUpperCorner().getOrdinate(0))).append(" ").append(envelope.getUpperCorner().getOrdinate(1)).toString());
  end("ows:WGS84BoundingBox");
}
