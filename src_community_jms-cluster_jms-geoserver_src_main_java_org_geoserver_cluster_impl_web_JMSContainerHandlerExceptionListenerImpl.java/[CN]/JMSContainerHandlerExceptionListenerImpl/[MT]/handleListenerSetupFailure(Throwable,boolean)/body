{
  if (session != null) {
synchronized (session) {
      if (session.isSessionInvalidated()) {
        return;
      }
      Session.set(session);
      if (fp != null) {
        if (alreadyRecovered) {
          fp.warn("There was an error which seems already fixed: " + ex.getLocalizedMessage());
        }
 else {
          fp.error(ex.getLocalizedMessage());
        }
      }
    }
  }
}
