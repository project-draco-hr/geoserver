{
  for (  PublishedInfo published : layerGroup.getLayers()) {
    if (published instanceof LayerInfo) {
      if (!isQueryable((LayerInfo)published)) {
        return false;
      }
    }
 else {
      if (!isQueryable((LayerGroupInfo)published)) {
        return false;
      }
    }
  }
  return true;
}
