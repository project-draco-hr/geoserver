{
  if (null == requestedVersion) {
    return VERSION_1_3_0;
  }
  if (VERSION_1_1_1.equals(requestedVersion)) {
    return VERSION_1_1_1;
  }
  if (VERSION_1_3_0.equals(requestedVersion)) {
    return VERSION_1_3_0;
  }
  if (requestedVersion.compareTo(VERSION_1_3_0) < 0) {
    return VERSION_1_1_1;
  }
  return VERSION_1_3_0;
}
