{
  EvaluationResult result=function.evaluate(inputs,context);
  if (result.indeterminate())   return new MatchResult(MatchResult.INDETERMINATE,result.getStatus());
  BooleanAttribute bool=(BooleanAttribute)(result.getAttributeValue());
  if (bool.getValue())   return new MatchResult(MatchResult.MATCH);
 else   return new MatchResult(MatchResult.NO_MATCH);
}
