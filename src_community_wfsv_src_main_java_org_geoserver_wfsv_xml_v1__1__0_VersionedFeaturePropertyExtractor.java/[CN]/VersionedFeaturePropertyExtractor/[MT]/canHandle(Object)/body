{
  try {
    if (!(object instanceof SimpleFeature) || object instanceof FeatureCollection)     return false;
    SimpleFeature f=(SimpleFeature)object;
    FeatureTypeInfo info=catalog.getFeatureTypeByName(f.getFeatureType().getName().getNamespaceURI(),f.getFeatureType().getTypeName());
    return info != null && info.getFeatureSource(null,null) instanceof VersioningFeatureSource;
  }
 catch (  Exception e) {
    LOGGER.log(Level.FINE,"Error occurred trying to determine versioning status of a feature type",e);
    return false;
  }
}
