{
  Resource f=loader.get("logging.xml");
  if (f != null) {
    XStreamPersister xp=new XStreamPersisterFactory().createXMLPersister();
    BufferedInputStream in=new BufferedInputStream(f.in());
    try {
      LoggingInfo loginfo=xp.load(in,LoggingInfo.class);
      return loginfo;
    }
  finally {
      in.close();
    }
  }
 else {
    return null;
  }
}
