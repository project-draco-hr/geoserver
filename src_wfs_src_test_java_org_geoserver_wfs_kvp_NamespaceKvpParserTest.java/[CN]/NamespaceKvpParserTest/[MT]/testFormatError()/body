{
  try {
    parser.parse("xmlns[bad=format]");
    fail("Expected IAE");
  }
 catch (  IllegalArgumentException e) {
    assertTrue(true);
  }
  try {
    parser.parse("xmlns(bad=http://format]");
    fail("Expected IAE");
  }
 catch (  IllegalArgumentException e) {
    assertTrue(true);
  }
  try {
    parser.parse("bad=http://format");
    fail("Expected IAE");
  }
 catch (  IllegalArgumentException e) {
    assertTrue(true);
  }
}
