{
  if (isNotEmpty(userName) == false)   throw createSecurityException(ROLE_ERR_04);
  if (services.length == 0)   return;
  for (  GeoServerUserGroupService service : services) {
    if (service.getUserByUsername(userName) != null)     return;
  }
  throw createSecurityException(ROLE_ERR_06,userName);
}
