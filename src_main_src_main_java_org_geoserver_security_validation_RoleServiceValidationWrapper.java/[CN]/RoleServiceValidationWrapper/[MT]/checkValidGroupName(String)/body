{
  if (isNotEmpty(groupName) == false)   throw createSecurityException(ROLE_ERR_05);
  if (services.length == 0)   return;
  for (  GeoServerUserGroupService service : services) {
    if (service.getGroupByGroupname(groupName) != null)     return;
  }
  throw createSecurityException(ROLE_ERR_07,groupName);
}
