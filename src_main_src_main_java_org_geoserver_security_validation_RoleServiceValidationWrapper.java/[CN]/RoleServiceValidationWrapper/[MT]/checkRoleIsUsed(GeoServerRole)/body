{
  if (checkAgainstRules == false)   return;
  List<String> keys=new ArrayList<String>();
  for (  ServiceAccessRule rule : ServiceAccessRuleDAO.get().getRulesAssociatedWithRole(role.getAuthority()))   keys.add(rule.getKey());
  for (  DataAccessRule rule : DataAccessRuleDAO.get().getRulesAssociatedWithRole(role.getAuthority()))   keys.add(rule.getKey());
  if (keys.size() > 0) {
    String ruleString=StringUtils.collectionToCommaDelimitedString(keys);
    throw createSecurityException(ROLE_IN_USE_$2,role.getAuthority(),ruleString);
  }
}
