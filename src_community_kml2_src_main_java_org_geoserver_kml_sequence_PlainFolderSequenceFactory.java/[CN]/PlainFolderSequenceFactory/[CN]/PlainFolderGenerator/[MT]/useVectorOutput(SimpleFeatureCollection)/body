{
  int kmscore=context.getWms().getKmScore();
  Object kmScoreObj=context.getRequest().getFormatOptions().get("kmscore");
  if (kmScoreObj != null) {
    kmscore=(Integer)kmScoreObj;
  }
  if (kmscore == 100) {
    return true;
  }
  if (kmscore == 0) {
    return false;
  }
  double magic=Math.pow(10,kmscore / 15);
  if (fc.size() > magic) {
    return false;
  }
 else {
    return true;
  }
}
