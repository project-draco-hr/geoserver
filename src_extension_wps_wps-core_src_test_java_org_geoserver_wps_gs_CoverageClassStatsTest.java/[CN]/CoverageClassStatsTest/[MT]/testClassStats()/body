{
  String xml="<?xml version=\"1.0\" encoding=\"UTF-8\"?>" + "<wps:Execute version=\"1.0.0\" service=\"WPS\" " + "    xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\" "+ "    xmlns=\"http://www.opengis.net/wps/1.0.0\" "+ "    xmlns:wps=\"http://www.opengis.net/wps/1.0.0\" "+ "    xmlns:ows=\"http://www.opengis.net/ows/1.1\" "+ "    xmlns:wcs=\"http://www.opengis.net/wcs/1.1.1\" "+ "    xmlns:xlink=\"http://www.w3.org/1999/xlink\" "+ "    xsi:schemaLocation=\"http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd\">"+ "  <ows:Identifier>ras:CoverageClassStats</ows:Identifier>"+ "  <wps:DataInputs>"+ "    <wps:Input>"+ "      <ows:Identifier>coverage</ows:Identifier>"+ "      <wps:Reference mimeType=\"image/tiff\" xlink:href=\"http://geoserver/wcs\" method=\"POST\">"+ "        <wps:Body>"+ "          <wcs:GetCoverage service=\"WCS\" version=\"1.1.1\">"+ "            <ows:Identifier>sf:sfdem</ows:Identifier>"+ "            <wcs:DomainSubset>"+ "              <ows:BoundingBox crs=\"http://www.opengis.net/gml/srs/epsg.xml#26713\">"+ "                <ows:LowerCorner>589980.0 4913700.0</ows:LowerCorner>"+ "                <ows:UpperCorner>609000.0 4928010.0</ows:UpperCorner>"+ "              </ows:BoundingBox>"+ "            </wcs:DomainSubset>"+ "            <wcs:Output format=\"image/tiff\"/>"+ "          </wcs:GetCoverage>"+ "        </wps:Body>"+ "      </wps:Reference>"+ "    </wps:Input>"+ "    <wps:Input>"+ "      <ows:Identifier>stats</ows:Identifier>"+ "      <wps:Data>"+ "        <wps:LiteralData>mean</wps:LiteralData>"+ "      </wps:Data>"+ "    </wps:Input>"+ "    <wps:Input>"+ "      <ows:Identifier>method</ows:Identifier>"+ "      <wps:Data>"+ "        <wps:LiteralData>QUANTILE</wps:LiteralData>"+ "      </wps:Data>"+ "    </wps:Input>"+ "    <wps:Input>"+ "      <ows:Identifier>noData</ows:Identifier>"+ "      <wps:Data>"+ "        <wps:LiteralData>-9.99999993381581251e+36</wps:LiteralData>"+ "      </wps:Data>"+ "    </wps:I