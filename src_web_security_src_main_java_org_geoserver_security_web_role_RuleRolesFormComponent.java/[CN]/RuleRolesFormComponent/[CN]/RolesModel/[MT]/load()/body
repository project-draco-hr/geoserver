{
  GeoServerRoleService roleService=GeoServerApplication.get().getSecurityManager().getActiveRoleService();
  List<GeoServerRole> roles=new ArrayList();
  for (  String roleName : roleNamesModel.getObject()) {
    try {
      roles.add(roleService.getRoleByName(roleName));
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
  return roles;
}
